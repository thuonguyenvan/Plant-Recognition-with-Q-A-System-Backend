      "ten": "Sơn dịch",
      "ten_khac": "Khoai ca, nam mộc hương",
      "ten_khoa_hoc": "Aristolochia indica L.",
      "ma_taxon": "464599",
      "ho": "Mộc hương (Aristolochiaceae)"
