  "@graph": [
    {
      "@type": "Plant",
      "ten": "Cỏ đậu hai lá",
      "ten_khac": "Đinh quý thảo, thương phòng.",
      "ten_khoa_hoc": "Zornia gibbosa Span.",
      "ma_taxon": "787020",
      "ho": "Đậu (Fabaceae)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây thảo, sống hằng năm hay sống dai, phân cành từ gốc, gốc phình thành củ thon nhỏ. Cành mảnh lúc đầu mọc ngả, sau đứng thẳng, dài 30 – 50 cm.",
      "lá": "Lá mọc đối có 2 lá chét hình trái xoan thuôn hay hình dải – mũi mác, gốc tròn, đầu có mũi nhọn ngắn; lá kèm hình mác nhọn có cựa ngắn.",
      "hoa": "Cụm hoa mọc ở kẽ lá thành bông thưa, lá bắc giống lá kèm nhưng rộng hơn, hoa màu vàng, đài có hai môi không giống nhau; tràng có cánh mang cựa, nhị 1 bó, không đều nhau.",
      "quả": "Quả đậu, thắt lại giữa các hạt, có lông hoặc nhẵn, đôi khi có vân mạng.",
      "mùa hoa": "tháng 4 – 6."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Chi Zornia J. Gmel. ở Việt Nam mới biết có hai loài. Loài cỏ đậu hai lá phân bố rải rác khắp các tỉnh, từ vùng trung du và đồng bằng Bắc Bộ vào đến Nam Trung Bộ và ở cả Tây Nguyên. Loài này cũng phân bố ở Ấn Độ, Mianma, Trung Quốc, Thái Lan, Xri Lanka và Indonesia.",
      "thường_mọc": "Cỏ đậu hai lá là cây thảo sống nhiều năm, thường mọc lẫn với các loại cỏ thấp ở ven đường đi, bãi sông, trên nương rẫy, ven rừng, chân đồi và rừng thưa rụng lá hoặc nửa rụng lá ở Tây Nguyên.",
      "Đặc điểm sinh trưởng": "Cây ưa sáng, ưa ẩm và có thể chịu được hạn, đồng thời cũng thích nghi được trên nhiều loại đất, kể cả đất cát pha, hơi chua và nghèo dinh dưỡng.  Cây ra hoa quả hàng năm, tái sinh tự nhiên chủ yếu từ hạt và mọc cây chồi khỏe sau khi bị cắt hay bị gia súc ăn phần cành và lá."
    },
    {
      "@type": "Thông tin khác",
      "thong_tin_khac": null
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Toàn bộ phần thân, cành mang lá; rễ. Dùng tươi hay phơi khô."
    },
    {
      "@type": "Thành phần",
      "thành phần": null
    },
    {
      "@type": "Tính vị",
      "Có vị": "Cỏ đậu hai lá có vị ngọt nhạt, tính mát, có công năng thanh nhiệt, giải biểu, tán ứ, tiêu sưng."
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Củ đậu hai là được dùng chữa cảm mạo, viêm họng, viêm kết mạc, viêm dạ dày, ruột cấp, rối loạn tiêu hoá, kiết lỵ, viêm gan vàng da, viêm vú cấp; trẻ em cam tích và suy dinh dưỡng.",
      "được_dùng": "Liều dùng mỗi ngày 15-30g (hoặc 30-60g), sắc lấy nước uống. Dùng ngoài còn lấy cây tươi, rửa sạch, giã nát, đắp lên chỗ đòn ngã tổn thương, nhọt, viêm da, sưng tấy.",
      "chữa": "Rễ cây cỏ đậu hai lá đốt thành than, tán nhỏ, chiều với nước, đắp lên chỗ ung nhọt, đinh độ. khi chưa vỡ mủ, có thể làm tiêu."
    },
