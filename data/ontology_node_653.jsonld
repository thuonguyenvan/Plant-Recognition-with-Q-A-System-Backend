    {
      "@type": "Plant",
      "ten": "Vương tùng",
      "ten_khac": "Cây củ khỉ, Hồng bì núi, Mắc mật, Móc mật, Mác mật, Dương tùng",
      "ten_khoa_hoc": "Clausena indica (Dalz.) Oliv",
      "ma_taxon": "940178",
      "ho": "Cam (Rutaceae)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây nhỡ hay cây gỗ nhỏ không lông, cao 3-7m, cành non màu tím đỏ, hay đen đen.",
      "lá": "Lá kép mọc so le, gồm 5-9 lá chét mọc so le hay mọc đối, mỏng, hơi cứng, hình trái xoan hay bầu dục, dài 3,5-5cm, rộng 1,7-3cm; gân phụ rất mảnh, 5-7 (10) cặp; có đốm nâu trên cả hai mặt; mép nguyên hoặc có răng cưa rất nhỏ; cuống phụ 4mm.",
      "hoa": "Ngù hoa cao 4cm ở ngọn nhánh, không lông. Hoa có 4 lá đài, 4 cánh hoa kết hợp màu trắng; nhị 8, 5 dài, 3 ngắn, chỉ nhị dẹp; bầu không lông.",
      "quả": "Quả xoan, cao 8mm, khi chín màu đỏ.",
      "mùa hoa": "tháng 4-6",
      "mùa quả": "tháng 9-1."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Cây mọc trên các núi đá vôi ở Cao Bằng, Lạng Sơn, Quảng Ninh, nhất là ở Thanh Hoá. Thu hái lá, rễ quanh năm, dùng tươi hay thái nhỏ phơi khô."
    },
