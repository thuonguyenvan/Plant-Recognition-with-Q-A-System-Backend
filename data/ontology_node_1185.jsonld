  "@graph": [
    {
      "@type": "Plant",
      "ten": "Xương bồ",
      "ten_khac": "Xương bồ, Thạch xương bồ",
      "ten_khoa_hoc": "Acorus gramineus Soland",
      "ma_taxon": "327370",
      "ho": "Ráy (Araceae)"
    },
    {
      "@type": "Mô tả",
      "lá": "Lá mọc đứng nhìn dải, dài 30-50cm, rộng 2-6mm, chỉ có gân giữa.",
      "hoa": "Hoa mọc thành bông mo ở đầu một cán dẹt dài 10-30cm, cán này được phủ bởi một lá bắc, lá bắc này dài 7-20cm, rộng từ 2-4mm vượt cao hơn cụm hoa rất nhiều, làm cho cụm hoa trông như lệch sang một bên, dài từ 5-12cm, đường kính 2-4mm.",
      "quả": "Quả mọng màu đỏ nhạt, một ngăn, có thành gần như khô. Quanh hạt có một chất gôm nhầy .",
      "cây": "Thạch xương bồ là một loại cỏ sống lâu năm, có thân rễ mọc ngang, đường kính ti bằng ngón tay, có nhiều đốt, trên có những sẹo lá.",
      "thân": "Thạch xương bồ là một loại cỏ sống lâu năm, có thân rễ mọc ngang, đường kính ti bằng ngón tay, có nhiều đốt, trên có những sẹo lá.",
      "mùa hoa": "Mùa hoa tháng 5-7",
      "mùa quả": "mùa quả tháng 6-8."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Trong thạch xương bồ có chừng 0,5-0,8% tinh dầu, trong tinh dầu có chừng 86% asaron C12H16O3, Ngoài ra còn có một chất phenol và axit béo. Tinh dầu có tỷ trọng 1,058 ở 150, chỉ số xà phòng 1,3 chỉ số este 5,79 độ sôi 140- 1600 ở 6mm thủy ngân. Trong thủy xương bồ có 1,5-3,5% tinh dầu, trong đó có thành phần chủ yếu cũng là asaron C12H16O3 (4 propenyl 1-3-5 trimetoxybenzol), rồi đến asarylandehyt C10H12O4. Ngoài ra còn một glucozit đắng gọi là acorin và chất tanin."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Xương bồ là một vị thuốc được dùng cả trong Tây y và Đông y. Tính vị xương bồ theo Đông y: Vị cay, tính ôn, vào hai kinh tâm và can. Tác dụng tẩy uế, khai khiếu, tuyên khí, trục đờm, dùng chữa thần kinh suy nhược, kém tiêu hóa, thông cửu khiếu, sáng tai mắt, ôn tràn vị, trị phong hàn tê thấp. Trẻ con sốt nóng nấu nước tắm khỏi. Phàm âm huyết không đủ, tinh hoạt, ra nhiều mồ hôi cấm dùng. Kỵ sắt, ghét ma hoàng, đọc đởm, thịt dê, đường và mật. Trên thực tế xương bồ được dùng làm thuốc bổ, bổ dạ dày, giúp sự tiêu hóa. Mới đây được dùng làm thuốc điều hòa nhịp tim trong mỗi nhịp xoang nhanh, ngoại tâm thu thành chuỗi, nhịp do huyệt xoang nút. Ngày dùng 4 đến 10g dùng hàng ngày, kéo dài 1-2 tháng. Dùng ngoài làm thuốc trừ sâu bọ, chống chấy rận. Tại Malaixia người ta tán thân rễ xương bồ rắc vào gốc cây có mối. Còn dùng làm bột thuốc đánh răng."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Thạch xương bồ và thủy xương bồ mọc hoang tại những miền núi phía Bắc và Trung nước ta, thường ở những nơi khe đá, khe suối, chỗ mát. Có thể thu hái quanh năm, nhưng tốt nhất vào mùa thu ở các tháng 8-9. Hái về cắt bỏ lá và rễ con, rửa sạch đất cát và phơi khô."
    },
    {
      "@type": "luu_y"
    }
  ]
