  "@graph": [
    {
      "@type": "Plant",
      "ten": "Han tía",
      "ten_khac": "Han tía, Nàng hai tía, Han lình",
      "ten_khoa_hoc": "Laportea violacea Gagnep.",
      "ma_taxon": "1581474",
      "ho": "Urticaceae (Gai)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây thảo hay bụi nhỏ, cao 2-4m.",
      "thân": "Thân có nhiều nhánh nhẵn, có sẹo lá hình mắt chim sát nhau.",
      "lá": "Lá có phiến xoan tròn, dài 5-12cm, có lông ngứa ngắn, mép có răng to; cuống dài 4-9cm; lá kèm 2mm.",
      "hoa": "Ngù hoa cùng gốc hay khác gốc, ở nách lá, có lông, dạng chuỳ 4-8 nhánh; cụm hoa đực dài 5cm; cụm hoa cái dài 15-20cm. Hoa đực có 5 lá đài, 5 nhị, nhuỵ cái lép; hoa cái có 4 lá đài.",
      "quả": "Quả bế hình trái xoan dẹp, có mụn trên bề mặt, vòi nhuỵ cong.",
      "mùa hoa": "Cây ra hoa vào mùa hè."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Cây thường gặp trên núi đá vôi tại Lạng Sơn, Thanh Hóa. Han tía là cây sinh trưởng mạnh trong mùa hè – thu; có khả năng tái sinh cây chồi sau khi bị chặt phá. Ra hoa quả nhiều hàng năm; hạt rơi vào các kẽ đá, hốc cây đều có khả năng nảy mầm. Han tía là loại cây ít được ưa chuộng vì lông tuyến của nó thường gây ngứa và dị ứng da. Cây trồng được bằng gốc (còn rễ) hoặc cây con mọc lên từ hạt."
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Rễ, thu hái quanh năm, phơi khô."
    },
    {
      "@type": "Tính vị",
      "Có vị": "Theo kinh nghiệm nhân dân, rễ cây han tía có tác dụng giảm đau, chống co thắt."
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Lông rất ngứa; nhưng rễ được dùng làm thuốc trừ giun, ỉa ra máu và làm dễ tiêu hoá.",
      "chữa": "Rễ han tía được dùng chữa tê thấp, hen phế quản với liều 8-10g.",
      "được_dùng": "Chữa tê thấp: Rễ han tía 40g, vỏ thân ngũ gia bì 12g. Hai vị thái nhỏ, phơi khô, ngâm rượu, uống mỗi lần một chén nhỏ. Ngày 2 lần.",
      "làm thuốc": "Chữa hen phế quản: Rễ han tía 10g, củ ráy 10g, vỏ quả bưởi đào hay vỏ quít 20g. Tất cả thái nhỏ, phơi khô sao vàng, sắc với 400ml nước còn 100ml, uống làm 2 lần trong ngày. Dùng 5-7 ngày liền."
    }
  ]
