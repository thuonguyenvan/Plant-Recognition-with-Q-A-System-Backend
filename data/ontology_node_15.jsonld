  "@graph": [
    {
      "@type": "Plant",
      "ten": "Cam thảo nam",
      "ten_khac": "Cam thảo đất, Cam thảo nam, Dã cam thảo, Thổ cam thảo, Trôm lay (Kho), Dạ kham (Tày)",
      "ten_khoa_hoc": "Scoparia dulcis",
      "ma_taxon": "317963",
      "ho": "Scrophulariaceae (Hoa mõm chó)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây cam thảo đất (hay còn gọi là cam thảo nam), là một loại cây thân thảo hoặc bán cây bụi thuộc họ Plantaceae và chi Scoparia, có nguồn gốc từ châu Á và châu Âu.",
      "thân": "Cam thảo nam là một loại cỏ mọc thẳng đứng, cao 30-80cm, thân nhẵn, rễ to hình trụ.",
      "lá": "Lá đơn, mọc đối hoặc vòng 3 lá một. Phiến lá hình mác hay hình trứng ngược, dài 1,53cm, rộng 8- 12mm, phía cuống hẹp lại thành cuống ngắn, mép lá nửa phía trên có răng cưa to, phía dưới nguyên.",
      "hoa": "Hoa nhỏ màu trắng ở kẽ lá, mọc riêng lẻ hoặc thành đôi.",
      "quả": "Quả nhỏ hình cầu, trong chứa nhiều hạt nhỏ.",
      "mùa hoa": "Mùa hạ."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Toàn cây chứa fredelin, beta-glutinol, alpha-amyrin, axit betulinic, axit iflaionic, axit Dulcioic, axit Scoparic A, B, C, Scoparinol, Dulic-nol, 6-methoxybenzoxazolinene), 5,7- dihydroxy-3′,4′,6,8-tetramethoxyflavon (5,7-dihydroxy-3′,4′,6,8-tetramethoxyflavon), 5,7,8, 3′,4′,5′-hex- ahydro xyflavone-7-O-β-D-glucuronide (5,7,8,3′,4′,5′-hex-ahydro xyflavone), Mignonette Luteoline, 6,8-di-C-glucosyl apigenin (vicenin- 2), linarin, vitexin, isovi-texin, Alpine baicalin (scutellarin), scutellarin methyl ester, luteolin-7-glucoside, acacetin, axit p-coumaric, rượu cam thảo hoang dã (dulciol), amelin (amellin).",
      "gồm chất": "Phần trên mặt đất có chứa một chất dầu sền sệt mà trong thành phần có dulciol, scopariol, (+)manitol, glucose.",
      "chứa": "Lá chứa glycyrrhizinol, β-myxenol, 6-methoxybenzoxazolinone, robinin, axit glycyrrhizic B và axit glycyrrhizinic A.",
      "tìm thấy": "Rễ chứa β-sitosterol, hexacosanol, D-mannitol, 6-methoxybenzoxazolinone, axit ephric, axit betulinic và Coixol."
    },
    {
      "@type": "Tính vị",
      "Có vị": "Vị ngọt, tính mát"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Công năng, chủ trị : Bổ tỳ, sinh tân, nhuận phế, thanh nhiệt giải độc, lợi niệu.",
      "chữa": "Giải độc do say sắn, rong kinh, ban chẩn, phế nhiệt gây ho, viêm họng, tiêu chảy, thấp cước khí, rôm sởi trẻ em, chàm (thấp chẩn)."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Dùng tươi: Ngày dùng từ 20 g đến 40 g. Dùng khô: Từ 8 g đến 12 g, dịch ép. Dạng thuốc hãm hoặc thuốc sắc.",
      "sắc uống": "Trị cước khí phù thũng (hai chân phù nề, ứ nước): Cam thảo nam 40g, Đường đỏ 40g. Sắc uống trước bữa ăn, ngày 2 lần.",
      "sắc nước uống": "Phòng trị ma chẩn (ban sởi): Cam thảo nam sắc uống thay trà, uống liên tục 3 ngày.",
      "giã nát": "Trị thấp chẩn (eczema), mụn rộp: Cam thảo nam tươi giã vắt nước xoa ngoài."
    },
