  "@graph": [
    {
      "@type": "Plant",
      "ten": "Tô hạp hương",
      "ten_khac": "Tô hạp hương",
      "ten_khoa_hoc": "Liquidambar orientalis Mill.",
      "ma_taxon": "356068",
      "ho": "Hamamelidaceae (Sau sau)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây tô hạp hương là một cây to có thể cao 12-15m.",
      "lá": "Lá hình chân vịt, cuống dài, có 3 hoặc 5 thuỳ mép có răng cưa.",
      "hoa": "Hoa đơn tính mọc ỏ kẽ lá, hoa đực thành bông, hoa cái họp thành hình cầu, trong có hạt dẹt.",
      "thân": "Trong vỏ cây có chất nhựa tức là tô hạp hương."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Phần đặc gồm có chất nhựa do cồn resitanola và axit xinamic.",
      "gồm chất": "Phần lỏng gồm 17-23% axic xinamic tự do, 24% este của axit xinamic (xinamat xinamyl hay styraxin xinamat etyl, xinamat phenylpropyl).",
      "Các hợp chất": "Axit xinamic tự do hay kết hợp chiếm chừng 47% nhựa lỏng.",
      "tinh dầu chứa": "Ngoài ra còn tinh dầu trong tinh dầu trong đó chủ yếu gồm styrolen (xinamen hay phenyletylen) và các ête xinamic, trong nhựa còn có cả vanilin.",
      "chứa": "Có nước quy định tô hạp hương phải chứa ít nhất 20% axit xinamic toàn bộ, chỉ số axit 60- 80%.",
      "dấu hiệu đặc trưng": "Chỉ số xà phòng 100-146. Dược thư y còn quy định tô hạp hương hoà vào cồn không được có quá 20% chất không tan."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Uống tô hạp hương giảm sự bài tiết phế quản.",
      "được_dùng": "Người ta còn dùng tính chất sát trùng của nó để chữa ghẻ, chữa rận dương vật, làm lên da non.",
      "làm thuốc": "Trong hương liệu, tinh dầu tô hạp hương dùng chế mùi thơm hoa xoan và chất-tô hạp hương tinh chế đuợc dùng làm chất định mùi với tên baumarome destyrax.",
      "chữa": "Ở nước ta mới chắc chắn có cây sau sau, cây sâu, sâu trắng Liquidambar formosana Hance cùng họ. Người ta thường nuôi sâu cước để lấy chỉ cước khâu nón lá. Khía thân cũng lấy dược một chất nhựa màu vàng nhạt, mùi thơm nhẹ. Trong nhựa này có axit xinamic, xinamyl xinamat và l.bocneola. Ở Việt Nam chưa thấy dùng làm thuốc (nhưng Trung Quốc dùng làm thuốc). Cây tô hạp hương bình khang-tô hạp Ahingia gracilipes Hemsl, phát hiện ở Điện Biên. Sau khi khía thân 2-3 ngày sẽ cho một thứ nhựa trắng dùng bôi lên giấy dán vào ngực khi bị ho."
    },
    {
      "@type": "Cách dùng",
      "mọc": "Cây này mọc ở châu Phi, Ấn Độ, Thổ Nhĩ Kỳ, Ba Tư.",
      "triển vọng": "Triển vọng có ở Việt Nam (tại nông trường Tràng Vinh, cách thị xã Hải Ninh 5km), tỉnh Quảng Ninh.",
      "thu hái": "Cách thu hái: Đầu mùa hạ chặt vỏ cây tới gỗ, mùa thu bóc vỏ, đun sôi với nước, lọc ép, nhựa dầu sẽ nổi lên. Có thể lấy như lấy cánh kiến trắng.",
      "vị thuốc": "Vị thuốc là một chất sền sệt như mật thường chia thành 2 lớp: Lớp dưới xám, lớp trên lỏng màu sẫm. Mùi thơm như bông, tôluy, vị đắng, hơi hắc. Vì tan trong cồn cho nên có thể tinh chế được (để loại tạp chất vô cơ và vỏ cây), tan trong clofofoc, axit axetic, cacbon sunfua, ête. Đun trên một phiến kính, để nguội sẽ có tinh thể styraxin và những tinh thể axit xinamic."
    },
    {
      "@type": "luu_y"
    }
  ]
