  "@graph": [
    {
      "@type": "Plant",
      "ten": "Bọ mẩy đỏ",
      "ten_khac": "Bọ mẩy đỏ",
      "ten_khoa_hoc": "Clerodendrum fortunatum L.",
      "ma_taxon": "474009",
      "ho": "Verbenaceae (Cỏ roi ngựa)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây nhỏ, thân cành hình vuông, có lông mịn.",
      "lá": "Lá mọc đối hình mác, dài 6-15 cm, rộng 3-6 cm, gốc thuôn đầu nhọn, mép nguyên hoặc khía rang không đều, hai mặt nhẵn trừ gân ở mặt dưới, cuống lá dài 0,8 – 2 cm, có lông.",
      "hoa": "Cụm hoa mọc ở kẽ lá thành xim phân đôi, ngắn hơn lá, hoa 5-10 , màu trắng ,dài hình chuông, đài hình vuông phình ở đầu, trắng có lông, ống hình trụ, cánh hoa tù, nhị 4 hơi thò ra ngoài tràng, đính ở hông, bàu nhàu.",
      "quả": "Qủa mọng, hình cầu fifnhs trên đài tồn tại, khi chín màu đỏ.",
      "mùa hoa": "tháng 7-9",
      "mùa quả": "tháng 7-9"
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Bọ mẩy đỏ phân bố rải rác ở một số nơi thuộc Trung Quốc, Ấn độ, Malaysia và Philippin. Ở Việt Nam cây chưa được nghiên cứu kỹ.",
      "thường_mọc": "Cây ưa sáng và có thể hơi chịu hạn, thường mọc rải rác hay tập trung thành đám ở các bãi đất hoang. Bọ mẩy đỏ có khả năng tái sinh chồi khỏe sau khi bị chặt."
    },
    {
      "@type": "Thông tin khác",
      "thong_tin_khac": ""
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Rễ và toàn cây, thu hái quanh năm, rửa sạch , thái nhỏ, phơi khô."
    },
    {
      "@type": "Thành phần",
      "thành phần": "Bọ mẩy đỏ chứa alkaloid , các flavonoid một chất đắng elerodin, các chất cabruvin, queroetin được phân lập từ rễ."
    },
    {
      "@type": "Tính vị",
      "Có vị": "Bọ mẩy đỏ có vị đắng, tính lạnh, có tác dụng thanh nhiệt, tiêu viêm, chống ho, giảm đau."
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Bọ mẩy đỏ được dùng trị cảm , sốt, sốt rét, ho, viêm họng, viêm phế quản, lao phổi, đau dạ dày, đau thượng vị, hoàng đản, rối loạn kinh nguyệt , bệnh lậu. Ngày 15-30g rễ , lá hoặc toàn cây, sắc uống. Dùng ngoài cây tươi giã đắp chữa đòn ,ngã tổn thương, mụn nhọt, viêm mủ da, bỏng, rắn, bọ cạp cắn. Để chữa giun , đọt non bọ mẩy đỏ giã nát, ép lấy dịch, người lớn uống 1 thìa cà phê lúc đói, trẻ em uống nửa liều người lớn . Có tài liệu cho rằng elerodin là một hoạt chất đắng có tác dụng tẩy giun."
    },
    {
