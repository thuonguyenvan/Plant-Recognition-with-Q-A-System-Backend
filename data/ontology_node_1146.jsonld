  "@graph": [
    {
      "@type": "Plant",
      "ten": "Ké hoa đào",
      "ten_khac": "Ké hoa đào lá xẻ, Ké khuyết",
      "ten_khoa_hoc": "Urena lobata L.",
      "ma_taxon": "127638",
      "ho": "Malvaceae (Bông)"
    },
    {
      "@type": "Mô tả",
      "cây": "Ké hoa đào là một cây nhỡ cao chừng 1m, có cành mang nhiều lông mịn hình sao.",
      "lá": "Lá gần tròn, đường kính 4-6cm, có khi tới 9cm, gân lá hình chân vịt (gồm 3-7 gân), mép có răng cưa và chia thùy, đầu lá nhọn phía cuối bằng hay hơi bằng, mặt trên màu xanh, mặt dưới màu tro nhạt có nhiều lông, dài hình sao.",
      "hoa": "Hoa có cánh màu hồng, mọc đơn độc hay thành đôi ở kẽ lá, đường kính chừng 1,7cm.",
      "quả": "Qủa hình cầu dẹt, có lông, trên có những gai hình móc, đường kính 7-8mm, hạt có vân dọc và có lông gợn ngắn.",
      "mùa hoa": "mùa hạ",
      "mùa quả": "mùa thu."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Phần trên mặt đất của cây chứa mangiferin. Toàn cây chứa acid amin, hợp chất phenol, sterol",
      "Hạt chứa dầu béo": "13-14%."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Theo tài liệu cổ, ké hoa đào có vị nhạt, hơi ngọt, tính lương, không độc. Thường nhân dân dùng rễ và thân sao vàng sắc uống và chữa lỵ, lá dã nát đắp lên những chỗ đau sưng, hoặc rắn độc cắn. Ngày dùng 40-80g cây tươi hay 20-40g cây khô. Dùng ngoài không kể liều lượng",
      "chữa": "rong kinh, rong huyết: Ké hoa đào, mần tưới, chỉ thiên, mã đề, mỗi vị 20g sắc uống.",
      "chữa": "trẻ em tiêu hóa kém: Rễ tươi rửa sạch, giã, ép lấy nước, mỗi lần cho trẻ uống 4 thìa cà phê, ngày 2 lần, uống liền 4 ngày."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Cây ké hoa đào mọc hoang ở khắp nơi trong nước ta. Còn mọc ở Trung quốc, Malaixia, Phlipin. Người ta dùng toàn cây hoặc lá phơi khô hay dùng tươi. Thu hái tốt nhất vào các mùa hạ và mùa thu"
    },
    {
      "@type": "luu_y"
    }
