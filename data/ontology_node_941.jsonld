  "@graph": [
    {
      "@type": "Plant",
      "schema:url": "https://tracuuduoclieu.vn/nghe-bong.html",
      "ten": "Nghể bông",
      "ten_khac": "Nghể bông, Nghể bà, nghể hoa trắng, nghể đông",
      "ten_khoa_hoc": "Polygonum orientale L.",
      "ma_taxon": "78454",
      "ho": "Rau răm (Polygonaceae)",
      "Mô tả": {
        "lá": "Lá mọc so le, hình bầu dục – thuôn, dài 30 – 35cm, gốc tròn hay hơi hình tim, phiến hơi men theo cuống lá, đầu nhọn",
        "hoa": "Cụm hoa mọc ở ngọn thành bông dài, ngắt quãng ở phía dưới; lá bắc có lông dày; hoa 2 – 3 cái ở một mấu, bao hoa màu trắng; nhị 8",
        "quả": "Quả hình thấu kính, hơi tròn, tù ở gốc, đầu có mỏ ngắn, nhẵn bóng.",
        "cây": "Cây thảo, sống hàng năm, có lông tơ mềm.",
        "thân": "Thân hình trụ, cành tỏa rộng.",
        "cuống": "cuống lá dài 2-8 cm",
        "bẹ": "bẹ chìa có lông, sớm rụng",
        "mùa hoa": "tháng 6-7",
        "mùa quả": "tháng 6-7"
      },
      "Phân bố": {
        "phân_bố": "Nghể bông phân bố chủ yếu ở vùng nhiệt đới châu Á, gồm Ấn Độ, Indonesia, Thái Lan, Lào và Nam Trung Quốc.",
        "ở_vùng": [
          {
            "@type": "schema:Place",
            "tên": "vùng nhiệt đới châu Á"
          }
        ],
        "các_tỉnh": {
          "@type": "schema:ItemList",
          "mục": [
            {
              "@type": "schema:Place",
              "tỉnh": {
                "tên": "Việt Nam"
              }
            }
          ]
        },
        "thường_mọc": "Cây ưa sáng; và thường mọc trên các ruộng nước, nơi nước nông ven bờ sông suối và ao hồ.",
        "Đặc điểm sinh trưởng": "Nghể bông phân nhánh nhiều, hoa thường mọc ra ở đầu cành trên tất cả các nhánh, nhân giống tự nhiên chủ yếu từ hạt. Trong thời kỳ sinh trưởng mạnh, nếu bị cắt, cây vẫn có khả năng tái sinh."
      },
      "Thông tin khác": {
        "thong_tin_khac": ""
      },
      "Bộ phận dùng": {
        "bo_phan_dung": "Toàn cây và quả."
      },
      "Thành phần": {
        "thành phần": "Quả nghể bông chứa các flavonoid quercetin, taxifolin (CA. 114, 1991, 3481 r), orientosid tương tự như vitextin và orientin như luteolin (CA. 1960, 54, 5010 f).   Các flavonoid khác như digicitrin, 5, 3′ dihydroxy 3. 8. 4. 5 titramethoxy 6, 7 methylendioxy ílavon; exotidin; 3′ hydroxy 3, 5, 6, 7, 8. 4′, 5′ hepta methoxy flavon, 3′ hydroxy 3. 5. 8. 4′. 5′ pentamethoxy 6, 7, methylenedi hydroxy flavon, isoorientin, 3. 5. 8. 3′ tetramethoxy 6, 7, 4’, 5′ bismethylendioxy flavon (Wiley – The handbook of Natural Flavonoid vol I, 1999)."
      },
      "Tính vị": {
        "Có vị": "Hạt nghể bông có vị nhạt, tính mát, hơi lạnh có tác dụng hoạt huyết, tiêu tích, lợi niệu, giải độc, làm sáng mắt. Toàn cây có vị cay, ôn, có ít độc, có tác dụng khu phong, lợi thấp, hoạt huyết, chỉ thống, thanh nhiệt, tiêu viêm. Hoa cũng có tác dụng hoạt huyết, tiêu tích. Rễ cây vị cay, có độc, chỉ được dùng ngoài."
      },
      "Công dụng": {
        "cong_dung_y_hoc": "Toàn cây nghể bông được dùng chữa thấp khớp, đầu gối sưng đau, bệnh về mắt. Ngày 20 ~ 30g sắc uống. Dùng ngoài cây tươi nấu nước rửa hoặc giã nát đắp chữa lở ngứa, mày đay, nhọt độc, côn trùng đốt.  Quả chữa đau bụng, đau dạ dày, táo bón, khó tiêu, chướng bụng, xơ gan cổ trướng, mắt phát hỏa, mụn nhọt, tràng nhạc, áp xe. Ngày 6 – 9g, hoặc hơn dưới dạng bột, sắc uống hoặc ngâm rượu. Rễ được dùng nấu nước, ngâm rửa chữa sưng chân, cước khí, tê thấp, lở ngứa.  Lá và đọt non có vị chua, được dùng làm rau ăn."
      },
      "Cách dùng": {
        "được_dùng": "Chữa viêm gan, xơ gan cổ trướng: Quả nghể bông 15g, đại phúc bì (vỏ ngoài và vỏ giữa của quả cau hoặc quả cau bỏ hạt) 12g, hắc sửu (hạt đen của cây bìm bìm) 9g, sắc uống. Chữa tràng nhạc, ung nhọt, áp xe: Quả nghể bông, ngưu tất, trạch lan, mỗi vị 9g, sắc uống. Chữa mắt kém, cải thiện thị giác, làm sáng mắt: Quả nghể bông, hoàng cầm, mỗi vị 9g; cúc hoa 12g, sắc uống. Chữa thấp khớp, lưng gối đau mỏi: Toàn cây nghể bông 30g, cóc mẳn 15g, sắc uống.  *Nguồn : cây thuốc và động vật làm thuốc ở Việt Nam."
      },
      "luu_y": {
        "luu_y": ""
      }
    }
