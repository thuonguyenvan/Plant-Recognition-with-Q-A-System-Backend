    {
      "@type": "Plant",
      "ten": "Tràm",
      "ten_khac": "Tràm lá dài, tràm lá hẹp, chè đồng, chè cay, bạch thiên tầng.",
      "ten_khoa_hoc": "Melaleuca leucadendra (L.) L.",
      "ma_taxon": "1491620",
