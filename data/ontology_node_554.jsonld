  "@graph": [
    {
      "@type": "Plant",
      "ten": "Cây rau mùi",
      "ten_khac": "Rau mùi hồ tuy, Hương tuy, Nguyên tuy, Ngò, Ngổ, Ngổ thơm, Coriandre, Coriander (Anh), Korianđer (Đức).",
      "ten_khoa_hoc": "Coriandrum sativum L.",
      "ma_taxon": "67759",
      "ho": "Hoa tán (Apiaceae)."
    },
    {
      "@type": "Mô tả",
      "cây": "Cây sống hằng năm, cao 0,35-0,50m, thân nhẵn, phía trên phân nhánh.",
      "lá": "Lá ở gốc có cuống dài, 1 đến 3 lá chét, lá chét hình hơi tròn và bản thân lại thường xẻ thành 3 thùy, mép thùy có khía răng tròn và to; những lá phía trên có lá chét chia thành những thùy hình sợi nhỏ, nhọn. Toàn thân và lá vò có mùi thơm đễ chịu.",
      "hoa": "Hoa trắng hay hơi hồng, họp thành tán gồm 3-5 gọng, không có tổng bao, tiểu bao gồm 2-3 lấ chét đính ở một phía.",
      "quả": "Quả bế đôi hơi hình cầu, nhẩn, dài 2,5-4mm, gồm 2 nửa (phân liệt quả), mỗi nửa có 4 sống thẳng và hai sống chung cho cả 2 nửa."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Lá và toàn cây."
    },
    {
      "@type": "Thành phần",
      "thành phần": "Trong quả mùi có 0,3 đến 0,8, có khi tới 1% tinh dầu. Ngoài ra còn 13-20% chất béo, 16-18% chất protein, 3 8% xenluloza và 13% chất không nitơ.",
      "gồm chất": "Thành phần chủ yếu của tinh dầu là 70-90% linalola quay phải (còn gọi là coriandrola, 5% d.pinen, limonen, tecpinen, mycxen, phelandren, một ít geraniola và bocneola). Trong lá và thân cũng chứa trên dưới 1% tinh dầu."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Quả mùi là một vị thuốc được dùng trong đông y và tây y. Tây y dùng quả mùi làm thuốc trung tiện, làm dễ tiêu, kích thích và giúp tiêu hóa. Công nghiệp nước hoa, hương liệu cho chè và rượu mùi dùng nhiều hơn nữa.",
      "có tác dụng": "Tính chất quả mùi theo tài liệu cổ: Vị cay, tính ôn, có tác dụng phát tán, thúc đậu sởi cho mọc, trừ tà khí, khu phong, long đờm, dùng làm thuốc mạnh dạ dày, tiêu cơm, thông khí ở bụng dưới, lợi đại tiểu trường, sởi, đậu không mọc. Phàm chứng cước khí, kìm sang, sâu răng hôi mồm chớ ăn.",
      "được_dùng": "Hiện nay quả mùi đuợc dùng thúc đậu sởi mọc: Tán quả mùi, hòa vào rượu mà phun thì đậu sởi mọc ngay. Ngoài ra còn dùng làm thuốc giúp sự tiêu hóa, chữa ho, ít sữa.",
      "sắc uống": "Mỗi ngày dùng 4-10g quả mùi, hoặc 10-20g lá hoặc cây tươi dưới dạng thuốc sắc hoặc ngâm rượu.",
      "chữa": "Đơn thuốc có quả mùi trong nhân dân  Đậu sởi không mọc: Quả mùi 80g tán nhỏ, rượu 100 ml, nước 100 ml. Đun sôi, đậy kín tránh bay hơi. Lọc bỏ bã. Phun từ đầu đến chân, trừ mật. Đậu sẽ mọc ngay (kinh nghiệm dân gian). Đẻ xong cợn sữa: Quả mùi 6g, nước 100ml. Đun sôi trong 15 phút, chia hai lần uống trong ngày. Mặt có những nốt đen: Quả mùi sắc nước rửa luôn, nốt đen sẽ mất dần. Lòi dom: Quả mùi đốt hun lấy khói hứng vào nơi dom lòi ra."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Cây mùi được trồng phổ biến ở khắp nước ta nhưng chỉ thấy để lấy lá làm gia vị hay một số ít đùng trong ngày tết nấu nước tấm cho thơm. Tai nhiều nước vùng ven Địa Trung Hải, Trung Á, Ấn Độ, Trung Quốc người ta trồng đại quy mô để lấy quả làm thuốc và cất tinh dầu dùng trong công nghiệp nước hoa.",
      "phơi khô": "Cây mùi ưa đất kiềm, mát, dễ hút nước, cày bừa kỹ, tránh nơi đất sét và râm mát vì cây mùi ưa ánh sáng. Quả chín tới đâu thu hái tới đấy để tránh cho những quả chín quá khỏi rụng. Hái toàn tán, phơi nắng cho khô rồi đập lấy quả, tiếp tục phơi nắng cho khô và bảo quản tránh ẩm. Khi khô, quả mùi mất mùi hôi và trở thành thơm dễ chịu. Ngoài quả ra, người ta còn dùng cả rễ và lá làm thuốc."
    },
    {
      "@type": "luu_y"
    }
  ]
