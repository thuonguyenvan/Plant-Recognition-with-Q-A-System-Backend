    {
      "@type": "Plant",
      "ten": "Biến hoá",
      "ten_khac": "Biến hoá, Thổ tế tân, Đỗ hành, Quán chì (HMông)",
      "ten_khoa_hoc": "Asarum caudigerum Hance",
      "ma_taxon": "709006",
      "ho": "Aristolochiaceae ( Mộc hương nam)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây thân thảo, sống dai, cao 30-50cm, bò lan trên mặt đất.",
      "thân": "Thân rễ nằm ngang dưới đất. Thân trông như có đốt do vết lá rụng còn sẹo. Từ thân rễ mọc lên 1-2 lá, có cuống dài 20- 30cm, phiến lá hình tim màu xanh đậm, hơi tía, bóng nhẵn, dài 10-15cm, cuống có lấm chấm màu tím, bẻ dễ gãy.",
      "lá": "Hai mặt lá đều có lông, mặt dưới nhạt.",
      "hoa": "Hoa sinh ra từ gốc, mọc riêng lẻ, hình loa kèn, tràng hoa màu tím.",
      "quả": "Quả màu nâu đen khi chín, trong chứa nhiều hạt cứng."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Chưa thấy tài liệu nghiên cứu."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Biến hoá mới thấy dùng trong phạm vi kinh nghiệm nhân dân làm thuốc chữa ho, ho có đờm, ho gà, có người dùng làm thuốc bổ làm cho da dẻ hồng hào. Ngày dùng 20-40g dưới dạng thuốc sắc hoặc tán bột uống. Có khi còn dùng chữa thấp khớp.",
      "chữa": "Chữa ho khan, rát cổ, hoặc ho có đờm: Biến hoá cả cây lá và rễ 40g, thêm nước vào sắc kỹ. Chia hai ba lần uống trong ngày. Nên uống lúc thuốc còn nóng. Uống liên tục trong 5-7 ngày."
    },
    {
      "@type": "Cách dùng",
      "mọc": "Cây mọc hoang dại ở những vùng cao, lạnh, chỗ bờ suối ẩm mát, có tán che, thường ở những khu vực núi đá ẩm thấp có rêu như Tam Đảo, Ba Vì, Cao Bằng, Lạng Sơn không chịu được vùng nóng thấp.",
      "mùa hoa": "tháng 3-4",
      "mùa quả": "tháng 5-6",
      "được_dùng": "Người ta thu hái toàn cây: Gốc, rễ và lá, có khi chỉ thu hái rễ. Mùa thu hái gần như quanh năm, nhưng tốt nhất vào thu đông. Hái về rửa sạch thái nhỏ phơi hay sấy khô."
    },
    {
      "@type": "luu_y"
    }
