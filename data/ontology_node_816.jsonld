    {
      "@type": "Plant",
      "ten": "Quỳnh",
      "ten_khac": "Quỳnh, Hoa quỳnh",
      "ten_khoa_hoc": "Epiphyllum oxypetalum (DC.) Haw.",
      "ma_taxon": "85410",
      "ho": "Cactaceae (Xương rồng)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây bụi mọng nước mọc đứng, có thân cứng cao 2-3m, các nhánh dẹp, mỏng, khía tai bèo.",
      "hoa": "Hoa to, màu trắng, dài 30cm, mọc thòng xuống, mùi thơm. Phiến hoa nhiều, xếp theo đường xoắn ốc, các phiến trong màu trắng, nhị xếp hai dãy, màu trắng, vòi và đầu nhuỵ màu trắng. Hoa vào tháng 6-8."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Gốc ở Trung và Nam Mỹ (Mêhicô đến Brazin), được trồng chủ yếu làm cảnh, có hoa đẹp và nở về đêm. Thu hái hoa khi nở, dùng tươi hoặc phơi khô. Thu hái thân quanh năm, dùng tươi."
    },
    {
      "@type": "Thông tin khác",
      "thong_tin_khac": null
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Hoa và thân –\u00a0Flos et Caulis Epiphylli Oxypetali."
    },
    {
      "@type": "Thành phần",
      "thành phần": null
    },
    {
      "@type": "Tính vị",
      "Có vị": null
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Hoa thường được dùng chữa: Lao phổi với ho ra máu; Tử cung xuất huyết; Viêm hầu. Dùng 3-5 hoa, dạng thuốc sắc. Dùng ngoài trị đinh nhọt, giã thân và đắp lên chỗ đau. Ở Vân Nam (Trung Quốc) toàn cây được dùng trị đòn ngã tổn thương, đau tâm vị (tâm vị khí thống), thổ huyết, phổi kết hạch."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Lao phổi với ho, ho ra máu:\u00a0Hoa Quỳnh 3-5 hoa, đường 15g, sắc uống. Tử cung xuất huyết:\u00a0Hoa Quỳnh 2-3 cái, nấu với thịt lợn chưng cách thủy làm thức ăn."
    },
