    "@graph": [
        {
            "@type": "Plant",
            "ten": "Ích trí nhân",
            "ten_khac": "Riềng lá nhọn, Ích trí, Riềng thuốc, Ích trí nhân",
            "ten_khoa_hoc": "Alpinia oxyphylla Miq.",
            "ma_taxon": "544838",
            "ho": "Zingiberaceae (Gừng)"
        },
        {
            "@type": "Mô tả",
            "cây": "Cây thảo, sống nhiều năm, cao 1 – 3m.",
            "thân": "Thân rễ mọc bò ngang. Thân khí sinh mảnh.",
            "lá": "Lá mọc so le, hình mác, dài 17 – 33 cm, rộng 3 – 6 cm.",
            "hoa": "Cụm hoa mọc tận cùng thành chùm; đài hình trụ có lông, chia 3 răng; tràng có ống dài khoảng 1 cm, có ba cánh hình bầu dục, màu trắng có đốm đỏ ở giữa.",
            "quả": "Quả nang, hình trứng hoặc trứng thuôn, đôi khi gần hình cầu, có nhiều nếp nhăn dọc, đầu có mũi nhọn, khi chín màu hồng hoặc đỏ; hạt có cạnh, đầu tù, màu nâu đen.",
            "mọc": "Toàn cây có mùi thơm đặc biệt.",
            "mùa hoa": "tháng 3 – 5",
            "mùa quả": "tháng 6 – 9."
        },
        {
            "@type": "Phân bố",
            "phân_bố": null
        },
