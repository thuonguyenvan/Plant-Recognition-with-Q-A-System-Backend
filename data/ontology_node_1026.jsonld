        {
            "@type": "Plant",
            "ten": "Cóc kèn",
            "ten_khac": "Cóc kèn, Cóc kèn nước",
            "ten_khoa_hoc": "Derris trifoliata Lour.",
            "ma_taxon": "354252",
            "ho": "Fabaceae (Đậu)",
            "Mô tả": {
                "lá": "Lá kép, thường có 3-5 (ít khi 7) lá chét xoan dài 5-10cm, rộng 2-4cm, chóp nhọn, gốc tròn, không lông.",
                "hoa": "Hoa mọc thành chùm đứng ở nách lá. Hoa trắng, ửng hồng, dài 12mm, đài hoa trăng trắng.",
                "quả": "Quả tròn 3-4cm, xám rồi vàng, chứa 1 hạt màu vàng hung.",
                "cây": "Dây leo mọc cao.",
                "mùa hoa": "tháng 8."
            },
            "Phân bố": {
                "phân_bố": "Loài của miền Malaixia – Châu Ðại Dương, mọc hoang dọc theo các sông rạch ở các nơi có nước mặn. Thu hái dây lá, rễ, hạt quanh năm. Rễ đào về, rửa sạch thái mỏng, phơi khô xay thành bột."
            },
            "Thông tin khác": {
                "thong_tin_khac": null
            },
            "Bộ phận dùng": {
                "bo_phan_dung": "Dây, lá, hạt, rễ – Caulis, Folium, Semen et Radix Derris Trifoliatae."
            },
            "Thành phần": {
                "thành phần": "Rễ cây chứa alcaloid và glucosid. Trong rễ có tới 0,47% rotenon, từ 1,2 đến 1,9% một chất ether hoà tan."
            },
            "Tính vị": {
                "Có vị": null
            },
            "Công dụng": {
                "cong_dung_y_hoc": "Cây được dùng chữa sốt rét kinh niên, huyết ứ, đàm ngưng sinh ra thũng trướng, trị ho và kiết lỵ. Quả chữa đau răng, bạch đới hạ. Rễ dùng sát trùng vết thương và làm thuốc diệt ruồi."
            },
            "Cách dùng": {
                "được_dùng": "Thường dùng dây sắc uống. Lá dùng ngoài giã đắp. Quả tán bột chữa đau răng, sắc uống thì chữa bạch đới hạ. Bột rễ rang nóng tẩm rượu bọc vùng đau nhức của vết thương trầy sứt không chảy máu. Bột rễ trộn với nước cơm dùng diệt ruồi muỗi. Nhân dân dùng lá phơi khô đặt trong các chum vại và mảng trữ thóc để trừ mọt."
            },
