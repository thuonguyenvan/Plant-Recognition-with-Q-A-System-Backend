  "@graph": [
    {
      "@type": "Plant",
      "ten": "Cỏ đuôi chồn",
      "ten_khac": "Thỏ vĩ thảo, đuôi chồn chân thỏ",
      "ten_khoa_hoc": "Uraria lagopodioides (L.) Decne ex DC.",
      "ma_taxon": "370067",
      "ho": "Đậu (Fabaceae)",
      "description": {
        "@type": "Mô tả",
        "cây": "Cây thảo cứng, cao 20 – 50 cm, phân cành từ gốc.",
        "thân": "Thân và cành mềm, hình trụ, khi non màu lục và có lông nhung, sau nhẫn và màu đỏ nhạt.",
        "lá": "Lá kép mọc so le, 1 – 3 lá chét, cuống chung dài 1,5 – 3 cm, có lông mềm; lá chét hình trái xoan hay bầu dục, dài 2,5 – 5 cm, rộng 1,5 – 3 cm, gốc tròn hoặc gần hình tim, đầu tủ hoặc hơi khuyết, hai mặt màu lục nhạt và có lông nhất là ở mặt dưới; lá kèm hình tam giác nhọn, dài 4 – 5 mm.",
        "hoa": "Cụm hoa mọc ở đầu cành thành chùm trụ hoặc hình trứng, dài 3 – 5 cm; lá bắc hình mác nhọn. dài làm cho chùm hoa có nhiều lông như tóc: hoa có rất nhiều lông, mọc cong xuống, dài hình chuông, rất nhiều lông, răng không đều, tràng có cánh cờ hình bầu dục, cánh bên không cuống, cảnh thìa có móng dài; nhị 1 bó; bầu 2 ô.",
        "quả": "Quả đậu, nhẵn, chứa hai hạt có vân mạng.",
        "mùa hoa": "tháng 4 – 10."
      },
      "distribution": {
        "@type": "Phân bố",
        "phân_bố": "Chi Uraria Desv. trên thế giới có khoảng vài chục loài, ở Việt Nam có 12 loài. Cỏ đuôi chồn phân bố rải rác hầu như khắp các tỉnh trong cả nước, từ vùng núi thấp xuống vùng trung du, đồng bằng và ra các đảo lớn. Trên thế giới, loài này có ở Ấn Độ, Trung Quốc, Lào, Campuchia, Thái Lan, Malaysia, Indonesia, Philippin và Australia.",
        "thường_mọc": "Cây ưa sáng, có biên độ sinh thái khá rộng. nên có thể thấy mọc rải rác ở các đồi cây bụi thấp, ven rừng, trảng cỏ, nương rẫy cũ, rừng thưa rụng"
      },
