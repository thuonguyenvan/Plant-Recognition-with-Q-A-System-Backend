    {
      "@type": "Plant",
      "ten": "Bàm bàm",
      "ten_khac": "Bàm bàm, Đậu dẹt, Dây bàm, Mi ba (Tày)",
      "ten_khoa_hoc": "Entada phaseoloides (L.) Merr.",
      "ma_taxon": "162325",
      "ho": "Mimosaceae (Trinh nữ)"
    },
    {
      "@type": "Mô tả",
      "lá": "Lá kép lông chim 2 lần, mọc so le, tận cùng bằng một tua cuốn chẻ đôi, gồm 6-8 lá chét mọc đối, hình bầu dục, dài 4-6cm, rộng 2-3cm, gốc thuôn hoặc tròn, đầu tù, hai mặt nhẵn, mặt trên sẫm bóng.",
      "hoa": "Cụm hoa mọc ở kẽ lá thành bông mảnh và dài 15-20cm, có lông; hoa màu trắng; đài hình đấu, có lông hoặc nhẵn, có răng nhỏ; tràng có 5 cánh rời nhau; nhị 10; bầu thuôn nhẵn.",
      "quả": "Quả dẹt, to bản như lưỡi dao, dài đến 1m, mép dày, thắt lại giữa các hạt; hạt to, hình mắt chim, khá dày màu nâu bóng; cứng như sừng.",
      "cây": "Cây gỗ, thân leo cao.",
      "mùa hoa": "tháng 1-3",
      "mùa quả": "tháng 4-6."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Bàm bàm là cây phân bố rộng rãi khắp từ bắc chí nam. Cây thường leo lên các cây gỗ to ở rừng nguyên sinh hay rừng thứ sinh, dọc theo các bờ suối và thung lũng."
    },
    {
      "@type": "Thông tin khác",
      "thong_tin_khac": null
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Dây, thu hái quanh năm, rửa sạch, thái lát đó rồi phơi khô. Hạt, thu hái vào mùa đông, xuân, bóc vỏ luộc hoặc hấp, sao sấy khô, tán thành bột."
    },
    {
      "@type": "Thành phần",
      "thành phần": "Trong toàn cây chứa một thứ saponin, nhiều nhất trong vỏ, trong hạt, ít hơn trong gỗ.",
      "gồm chất": "Trong lá tươi hầu như không có hay có rất ít nên khó phát hiện.",
      "Các hợp chất": "Ngoài saponin, trong hạt còn chứa một ancaloit và một chất dầu béo (hàm lượng dầu béo khoảng 18%) màu vàng, không vị.",
      "chứa": "Chất ancaloit là mội chất độc mạnh đầu tiên gây liệt chi dưới, sau làm chết con vật với liều 250ml trên 1kg thể trọng."
    },
    {
      "@type": "Tính vị",
      "Có vị": null
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Vỏ cây dùng tắm và gội đầu thay xà phòng: Vỏ cây hái về cắt thành từng mảnh, đập nát, phơi hay sấy khô. Khi dùng ngâm vào nước sẽ được một thứ nước màu nâu đỏ, dùng tắm hay gội đầu. Gỗ tuy chứa ít saponin hơn nhưng cũng dùng được. Hạt gần chín phơi khô cũng dùng thay vỏ và gỗ.",
      "được_dùng": "Chữa nóng, sốt, sài giật trẻ em: Lá bàm bàm tươi 50g, phối hợp với lá găng trâu, lá chanh giã nhỏ, xát khắp người trẻ em như kiểu đánh gìó.",
      "làm thuốc": "Vỏ giã nát ngâm nước, dùng nước ấy tắm ghẻ, bã vỏ thì xát lên người vào những nơi ghẻ.",
      "chữa": "Một số nơi dùng hạt bàm bàm để đặt lên vết rắn cắn.",
      "có tác dụng": "Chữa phong thấp, bị thương đau nhức: Thân cây bàm bàm khô 30g, cốt toái bổ 20g, huyết giác 20g. Sắc uống.",
      "sắc uống": "Chữa nóng sốt: Lá tươi bàm bàm 50-100g, lá chanh, lá găng trâu, lá răng ngựa. Giã nhỏ, xát khắp người như kiểu đánh gió.",
      "giã nát": "Chữa ghẻ, lở ngứa: Vỏ cây bàm bàm giã nát ngâm nước, dùng tắm, xát lên người vào những chỗ ghẻ, lở ngứa."
    },
