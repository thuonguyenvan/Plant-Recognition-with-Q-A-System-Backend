  "@graph": [
    {
      "@type": "Plant",
      "ten": "Cóc kèn leo",
      "ten_khac": "",
      "ten_khoa_hoc": "Derris scandens Benth.",
      "ma_taxon": "1373957",
      "ho": "Đậu (Fabaceae)"
    },
    {
      "@type": "Mô tả",
      "cây": "Dây leo rất to, dài đến 30m.",
      "thân": "Thân cành có lông màu xám nhạt, sau nhẵn, có những bì không và rãnh dọc.",
      "lá": "Lá kép mọc so le, lá chét 9 – 19, cứng và dai, hình bầu dục hoặc thuôn, dài 25 cm, rộng 1,5 – 2 cm, gốc tròn, đầu nhọn, mặt trên nhẫn bóng, mặt dưới có lông rải rác màu hơi trắng, gần lá chằng chịt thành mạng lưới rõ.",
      "hoa": "Cụm hoa mọc ở kẽ lá thành chùm dài 8 – 40 cm, có lông màu hung; hoa 5 – 12, màu trắng hoặc hồng nhạt; lá bắc con xẻ đôi dưới đài hoa; đài hình phễu, mặt ngoài có lông, lá đài hơi khía răng, tràng có cánh cờ thuôn đảo, cánh bên có lông, có tại rõ, cánh thìa hình liềm, có tại; nhị 10, ẩn trong bao hoa; bầu có lông, 6 – 10 noãn.",
      "quả": "Quả dài 2,5 – 7 cm, rộng 1 – 1,5 cm, thuôn ở hai đầu, thắt lại giữa các hạt, có lông mềm, có cánh; hạt 1 – 4 hình thận, dài 8 mm, rộng 6 mm."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Chi Derris Lour, trên thế giới có khoảng 70 loài, phân bố chủ yếu ở vùng nhiệt đới và cận nhiệt đới châu Á.  Ở Việt Nam, chi này có 13 loài, loài cóc kèn leo mới chỉ thấy phân bố ở các tỉnh phía nam, như Lâm Đồng, Ninh Thuận, Bình Thuận, ngoại thành Hồ Chí Minh, An Giang, Kiên Giang (Phan Ke Lọc và J. E. Vidal, 2000) và một địa phương khác ở đồng bằng sông Cửu Long. Trên thế giới, cóc kèn leo được ghi nhận phân bố Ấn Độ, Xri Lanca, Bănglađét, Pakistan.",
      "thường_mọc": "Cóc kèn leo là cây ưa sáng, thường leo trùm lên một số cây bụi, cây gỗ nhỏ ở các bờ kênh rạch (vùng đồng bằng sông Cửu Long), bờ suối ở cửa rừng hoặc ở ven rừng kín thường xanh.",
      "Đặc điểm sinh trưởng": "Cây phân cành nhiều và chỉ có những cành tiếp xúc nhiều với ánh sáng mới ra hoa quả nhiều và hàng năm. Cóc kèn leo tái sinh tự nhiên chủ yếu bằng hạt."
    },
    {
      "@type": "Thông tin khác",
      "thong_tin_khac": ""
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Thân hoặc rễ."
    },
    {
      "@type": "Thành phần",
      "thành phần": "Trong loài cóc kèn leo có chứa 6 flavon và 6 diprennilisoflavon: dirissoflavon A – F (Phạm Hoàng Hộ, 2006 Cây có vị thuốc ở Việt Nam, tr.218).  Củ cóc kèn leo chứa scandenin (C26H20O6), nallanin (C26H26O5), chandanin (C24H30O5), acid lonchocarpic và acid robustic. Hai acid này chỉ tìm thấy trong củ loài cóc kèn leo ở châu Mỹ, không tìm thấy trong loài mọc ở Ấn Độ."
    },
    {
      "@type": "Tính vị",
      "Có vị": "Chưa có tài liệu để cập vì cây có độc."
    },
