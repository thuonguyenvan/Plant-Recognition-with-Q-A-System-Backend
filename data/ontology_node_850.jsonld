    {
      "@type": "Plant",
      "ten": "Mộc thông",
      "ten_khac": "Mộc thông, Dây khố rách, Hỏm sắm mía (Dao)",
      "ten_khoa_hoc": "Clematis smilacifolia Wall.",
      "ma_taxon": "940168",
      "ho": "Ranunculaceae (Mao lương)"
