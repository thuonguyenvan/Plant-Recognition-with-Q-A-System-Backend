  "@graph": [
    {
      "@type": "Plant",
      "ten": "Cỏ bợ",
      "ten_khac": "Cỏ bợ, Rau tần, Cỏ chữ điền, Tứ diệp thảo, Tần thái, Cỏ bốn lá, Phiắc vèn, Phiắc chắn (Thái), Thủy tần (Tày), Cỏ bợ nước",
      "ten_khoa_hoc": "Marsilea quadrifolia L.",
      "ma_taxon": "82570",
      "ho": "Marsileaceae (Cỏ bợ)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cỏ bợ là một loài cỏ mọc hoang ở những nơi ẩm hay ở dưới nước, có thân rễ bò mảnh, mang từng nhóm 2 lá một, cuống lá dài 5-15cm.",
      "lá": "Mỗi lá gồm 4 lá chét, xếp chéo chữ thập. tối đến, các lá chét rủ xuống.",
      "rễ": "Từ gốc mỗi nhóm lá phát ra một chùm rễ phụ.",
      "quả": "Bào tử quả rất bé, nằm ở gốc cuống lá chia làm nhi u ô ngang trong chứa bào tử nang lớn, sẽ sinh nguyên tản cái và nhiều bào tử nang nhỏ sẽ cho nguyên tản đực, mỗi ô đó tương đương với một ổ tử nang và có áo riêng của nó."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Trong cỏ bợ có xyclaudenol (Reinhold và cộng sự Progress in Phytochemistry I 1968, 669)"
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Còn là một vị thuốc dùng trong phạm vi nhân dân. Nhân dân Việt Nam có nơi hái về làm món rau ăn sống. có khi hái về sao vàng hoặc phơi khô, sắc đặc uống làm thuốc mát thông tiểu tiện, chữa bạch đới, khí hư, mất ngủ. Có nơi còn giã cây tươi, ép lấy nước uống chữa rắn độc, bã đắp lên những chỗ sưng đau, sưng vú, tắc tia sữa. Nhân dân Trung Quốc cũng dùng với những công dụng gần tương tự như ở ta. Liều dùng hằng ngày 20-30g. Cần chú ý nghiên cứu."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Cây cỏ bợ mọc hoang khắp nơi ở Việt Nam. Cây mọc cạnh ao, đầm, nơi ẩm thấp, đồng ruộng (chú ý phân biệt nó với cây chua me có 3 lá). Còn mọc hoang tại các tỉnh Trung Quốc, châu Âu và châu Mỹ. Khi dùng người ta hái toàn cây và dùng tươi, ít khi dùng khô."
    },
    {
      "@type": "luu_y"
    }
  ]
