  "@graph": [
    {
      "@type": "Plant",
      "ten": "Bần",
      "ten_khac": "Bần, Bần chua, Bần sẻ",
      "ten_khoa_hoc": "Sonneratia caseolaris (L.) Engl.",
      "ma_taxon": "196662",
      "ho": "Sonneratiaceae (Bần)"
    },
    {
      "@type": "Mô tả",
      "lá": "Lá hình trái xoan ngược hay trái xoan thuôn, thon hẹp thành cuống ở góc, cụt hay tròn ở chóp, dai, dài 5-10cm, rộng 35-45mm.",
      "hoa": "Hoa đơn độc ở ngọn, rộng 5cm, có cuống hoa ngắn và bậm.",
      "quả": "Quả mọng hơi nạc, đường kính 3cm hay hơn, cao 18-20mm. Hạt dạng cái đinh, dài 6-7mm.",
      "cây": "Cây gỗ nhỏ, nhẵn, có các nhánh có đốt, với 4 góc tù."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Cây của rừng ngập mặn, gặp dọc bờ biển nước ta. từ sông Bạch Ðằng, qua Nha Trang đến thành phố Hồ Chí Minh, Cần Thơ… Loài cây ưa sáng nơi có nước mặn ít nhất là một phần trong năm. Cây có những rễ thở (phế căn). Nở hoa vào tháng 3-4, sau mùa khô, trước mùa mưa; nở về đêm, nhờ dơi thụ phấn. Bần là cây chắn sóng, bảo vệ đất ở vùng ven biển."
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Lá, quả – Folium et Fructus Sonneratiae."
    },
    {
      "@type": "Thành phần",
      "thành phần": "Vỏ thân và gỗ chứa archin(emodin), archinin (chrysophanic acid) và archicin. Trong quả có chất màu, archin và archicin."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Người ta lấy quả chua ăn sống hay nấu canh cá. Cũng được sử dụng làm thuốc đắp vào chỗ viêm tấy vì bong gân. Ở Ấn Độ, người ta dùng dịch quả lên men làm thuốc ngăn chặn của chứng xuất huyết. Ta dùng lá giã ra, thêm tí muối, làm thuốc đắp tốt các vết thương đụng giập và vết thương nhẹ. Ở Malaixia, người ta giã lá lẫn với cơm làm thuốc đắp chữa bí tiểu tiện. Cây Bần còn có những công dụng khác như rễ thở dùng làm nút chai; cành làm cần câu và làm củi đun.  Ghi chú: Cây Bần trứng hay Bần ổi – Sonneratia ovata Bak., có vỏ tróc thành mảnh mỏng như vỏ ổi, lá hình bầu dục, mọc phổ biến ở các vùng rừng ngập mặn, cũng có quả vị chua thơm, thường được nhân dân dùng nấu canh chua."
    },
    {
      "@type": "Cách dùng"
    },
    {
      "@type": "luu_y"
    }
  ]
