  "@graph": [
    {
      "@type": "Plant",
      "ten": "Mồng tơi",
      "ten_khac": "Mồng tơi, Lạc qùy, Phiắc păng (Tày), Chàn mau nhây (Dao)",
      "ten_khoa_hoc": "Basella rubra L.",
      "ma_taxon": "159191",
      "ho": "Basellaceae (Mồng tơi)"
    },
    {
      "@type": "Mô tả",
      "cây": "Mồng tơi là một dây leo, sống hằng năm hay hai năm.",
      "thân": "Thân mọc cuốn, dài 1.50-2m. Thân có phân nhánh, màu xanh nhạt hoặc tím nhạt.",
      "lá": "Lá mọc so le, đơn, nguyên, mẫm, có cuống, phiến lá hình trứng, đầu nhọn, phía cuống bằng hay hơi hẹp lại, dài 3-1cm, rộng 2-6cm.",
      "hoa": "Cụm hoa hình bông mọc ở kẽ lá, nhỏ, màu trắng hay tím đỏ nhạt. Những bông ở phía trên dài và gầy hơn.",
      "quả": "Quả mọng, nhỏ hình cầu hay hình trứng, dài chừng 5-5mm, màu tím đen khi chín."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Theo Read (1936), trong rau mồng tơi có Vitamin A3, vitamin B3, chất saponin, chất nhầy và chất sắt."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Trong nhân dân, thường chỉ dùng rau mồng tơi nấu canh ăn cho mát, ít dùng làm thuốc. Nhưng trong sách cổ (Bản thảo cương mục của Lý Thời Trân) có ghi là rau mồng tơi có vị chua hàn, hoạt, không độc, chủ trị hoạt trung, tán nhú lợi đại tiểu trường. Nhân dân Indonesia dùng rau mồng tơi là thuốc chữa trẻ con bị táo bón, phụ nữ đẻ khó, nước ép quả dùng nhỏ mắt chữa đau mắt. Người ta còn dùng để nhuộm đỏ các loại mứt, nếu thêm chanh thì màu đậm hơn, nhưng không nên cho nhiều quá. Tại Trung Quốc có nơi người ta dùng rau mồng tơi giã đắp chữa vú sưng nứt, giải độc."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Cây này nguồn gốc ở các nước nhiệt đới của châu Á và châu Phi. Tại Việt Nam, cây này mọc hoang và được trồng cho leo hàng rào để lấy rau ăn. Người ta hái thân và lá vào mùa hạ và mùa thu."
    },
    {
      "@type": "luu_y"
    }
  ]
