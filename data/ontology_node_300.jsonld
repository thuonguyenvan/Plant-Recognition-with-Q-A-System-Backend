    {
      "@type": "Plant",
      "ten": "Cây Bã Thuốc",
      "ten_khac": "Bã thuốc",
      "ten_khoa_hoc": "Lobelia pyramidalis Wal",
      "ma_taxon": "903483",
      "ho": "Campanulaceae ( Hoa chuông)"
    },
    {
      "@type": "Mô tả",
      "lá": "Cây thảo sống dai cao 1-2,5m hay hơn; thân nhẵn, phân cành ở ngọn. Lá mọc so le, hình ngọn giáo hẹp hay thuôn hay thon ngược, dài 5-50cm, rộng 1-8cm, mép có răng mịn, có lông ở hai mặt, gân phụ 10 cặp.",
      "hoa": "Chuỳ hoa ở nách lá hay ngọn thân dài 45cm, cuống hoa dài 1-2cm, đài có các thuỳ hẹp có lông hay không, dài 2cm; tràng chẻ đến gốc, có lông mịn ở mặt trong, môi dưới xẻ 3 thuỳ hẹp, nhị 5; bầu 2 ô.",
      "quả": "Quả nang hình cầu đường kính 7-10mm, chứa nhiều hạt nhỏ màu vàng nâu nhạt, hình thấu kính.",
      "cây": "Cây thảo sống dai cao 1-2,5m hay hơn; thân nhẵn, phân cành ở ngọn.",
      "mùa hoa": "tháng 5-7.",
      "mùa quả": "tháng 5-7."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Cây mọc hoang ở rừng núi, bãi hoang ven đường, ven rừng, một số nơi ở Lào Cai (Sapa), Lai Châu (nhiều nơi). Cũng phân bố ở nhiều nước khác. Ấn Độ, Thái Lan, Lào, Inđônêxia, Philippin.  Có thể thu hái lá vào mùa xuân-hè, lấy nhựa, dùng tươi."
    },
    {
      "@type": "Thông tin khác",
      "thong_tin_khac": null
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Lá và rễ – Folium et Radix Lobeliae."
    },
    {
      "@type": "Thành phần",
      "thành phần": "Lá khô và chồi non của cây chứa lobelin với hàm lượng cao (0,29-0,39%) là nguồn chiết xuất nhóm alcaloid này. Hạt chứa chất độc acronarcotic."
    },
    {
      "@type": "Tính vị",
      "Có vị": null
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Ở Ấn Độ: Nước hãm lá dùng sát khuẩn, rễ dùng trị bò cạp đốt. Ta thường dùng nhựa lá chữa nhọt mủ, áp xe sưng tấy, ngày bôi 2-3 lần."
    },
