    {
      "@type": "Plant",
      "ten": "Cam xũng",
      "ten_khac": "Cam xũng, Đơn lưỡi hổ, Lưỡi cọp, Lưỡi hùm, Cam sũng",
      "ten_khoa_hoc": "Sauropus rostratus Miq.",
      "ma_taxon": null,
