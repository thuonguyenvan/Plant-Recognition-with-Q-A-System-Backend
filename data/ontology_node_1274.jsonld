    {
      "@type": "Plant",
      "ten": "Công Cộng",
      "ten_khac": "Xuyên tâm liên , Công cộng, Khổ diệp, Hùng bút, Khổ đởm thảo, Nguyễn cộng, Nhất kiến kỷ",
      "ten_khoa_hoc": "Andrographis paniculata (Burm.f.) Wall. ex Nees",
      "ma_taxon": "143423",
      "ho": "Acanthaceae (Ô rô)",
      "Mô tả": {
        "cây": "Cây nhỏ, thuộc thảo, mọc thẳng đứng, cao từ 0,30 – 8m, nhiều đốt, rất nhiều cành.",
        "lá": "Lá mọc đối, cuống ngắn, phiến lá hình trứng thuôn dài hay hơi hình mác, hai đầu nhọn, mặt nhẵn, dài 3- 12cm, rộng 1- 3cm, nguyên, mềm.",
        "hoa": "Hoa màu trắng, điểm hồng, mọc thành chùm ở nách lá hay đầu cành, thành chùy.",
        "quả": "Quả dài, 15mm, rộng 3,5mm, hơi nhẵn.",
        "hạt": "Hạt hình trụ, thuôn dài.",
        "mùa hoa": "tháng 9-10."
      },
      "Phân bố": {
        "phân_bố": "Phân bố  ở Ấn Độ, Giava, Malaixia, miền man Trung Quốc (Quảng Châu)",
        "ở_vùng": [
          {
            "tên": "Ấn Độ"
