    {
      "@type": "Plant",
      "ten": "Chóc Gai",
      "ten_khac": "Chóc gai, Ráy gai, Sơn thục gai, Cây cừa, Cây móp (Nam Bộ).",
      "ten_khoa_hoc": "Lasia spinosa (L.) Thw.",
      "ma_taxon": "425691",
      "ho": "Araceae (Ráy)",
      "description": {
        "@type": "Mô tả",
        "lá": "Lá mọc thẳng từ thân rễ, cuống lá dài, có gai. Lá non hình mũi tên, lá già xẻ lông chim, mép nguyên.",
        "hoa": "Cụm hoa là một bông mo mang hoa cái ở gốc, hoa đực ở trên.",
        "quả": "Quả mọng.",
        "cây": "Cây loại nhỏ, thân rễ nằm ngang, có nhiều gai.",
        "mùa hoa": "tháng 3-4.",
        "mùa quả": "tháng 3-4."
      },
      "phan_bo": {
        "@type": "Phân bố"
      },
      "thong_tin_khac": {
        "@type": "Thông tin khác"
      },
      "bo_phan_dung": {
        "@type": "Bộ phận dùng"
      },
      "thanh_phan": {
        "@type": "Thành phần",
        "thành phần": "Chưa thấy tài liệu nghiên cứu. Sơ bộ thấy thân rễ chứa những chất cho những vết tương tự với rất nhiều loại axit amin trên sắc ký lớp móng."
      },
      "tinh_vi": {
        "@type": "Tính vị"
      },
      "cong_dung_y_hoc": {
        "@type": "Công dụng",
        "cong_dung_y_hoc": "Vị thuốc nhân dân dùng chữa ho, đau họng, phù thũng, tê thấp, suy gan, di chứng do sốt rét. Ngày dùng 4 đến 16g dưới dạng thuốc sắc hay ngâm rượu. Dùng riêng hay phối hợp với một số vị thuốc khác như nghệ."
      },
      "cach_dung": {
        "@type": "Cách dùng",
        "được_dùng": "Cây mọc hoang dại ở khắp nơi ẩm ướt trên có tàn che: Ruộng nước, bờ ao, bãi rẫy, ven suối. Người ta thường dùng thân rễ thu hái vào mùa thu đông. Đào rễ về rửa sạch, thái mỏng phơi hay sấy khô. Có khi người ta ngâm với nước gừng, nước phèn trước khi phơi hay sấy khô. Thuốc tăng cường sinh lý Bài thuốc bổ huyết điều kinh"
      },
