    {
      "@type": "Plant",
      "ten": "Thùn mũn",
      "ten_khac": [
        "Cây chua meo (tên ở Lạng Giang, Bắc Giang)",
        "Cây phi tử",
        "Cây chua ngút-vốn vén, tấm cùi (Thổ)",
        "Xốm mun (Thái)"
      ],
      "ten_khoa_hoc": "Embelia laeta (L.) Mez",
      "ma_taxon": "519626",
      "ho": "Đơn nem (Myrsinaceae)",
      "Mô tả": {
        "cây": "Cây mọc hoang ở các đồi thành từng bụi cao chừng 1-2m.",
        "thân": "Thân cứng, màu tía tím thỉnh thoảng có cành đỏ tươi hơn, trông như hơi mốc. Có vạch dọc rất rõ, bì khổng nhỏ như hạt kê.",
        "lá": "Cành lá mọc so le, hình lưỡi mác, mặt dưới nhạt hơn, dài 4,5-5cm, rộng 1,5-2cm. Phiến ở phía cuống hẹp nhọn. Cuống ngắn 5-6mm.",
        "hoa": "Hoa màu vàng lục nhạt, mọc dày đặc ở cành, đài hình tam tam giác nhọn, thuôn tù, nhị bầu nhỏ, hình nón.",
        "quả": "Qủa hình cầu, có núm nhọn, khi chín màu đỏ, đường kính khoảng 6mm, chứa một hạt. Vị lúc đầu ngọt, sau chua và hơi tê tê.",
        "mùa hoa": "Cây thường ra hoa vào khoảng tháng 2 – 4",
        "mùa quả": "sau đó kết quả vào tháng 5 – 7."
      },
      "Phân bố": {
        "phân_bố": null
      },
      "Thông tin khác": {
        "thong_tin_khac": null
      },
      "Bộ phận dùng": {
        "bo_phan_dung": "Qủa chính phơi hay sấy khô của cây Thùn mũn. Khi dùng xát sạch vỏ, lấy hạt, phơi khô và tán nhỏ."
      },
      "Thành phần": {
        "thành phần": "Trong quả có tanin, tinh dầu và axit embelic (còn gọi là embelin hay embelon-dihydroxy 2-5-lauryl 3-benzo quinon 1-4) một chất có cấu tạo quinonic.",
        "Các hợp chất": "Embelic là bột kết tinh hình lá nhỏ, màu vàng kim nhũ ống ánh, không mùi vị, không tan trong nước, tan trong cồn, ether, coloroform, benzen, tan trong dung dịch hydroxyd natri loãng (cho màu đỏ nhạt) và đa số các dung môi hữu cơ.",
        "có hoạt chất": "Hoạt chất được dùng dưới dạng embelat amoni."
      },
