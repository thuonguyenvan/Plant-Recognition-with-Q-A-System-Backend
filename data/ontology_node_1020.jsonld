  "@graph": [
    {
      "@type": "Plant",
      "ten": "Cơm Rượu",
      "ten_khac": "Cơm rượu, Bưởi bung",
      "ten_khoa_hoc": "Glycosmis pentaphylla (Retz.) Correa.",
      "ma_taxon": "410428",
      "ho": "Cam (Rutaceae)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây nhỏ hay cây nhỡ, mọc thành bụi, cao 3 – 5m.",
      "thân": "Cành màu lục pha tím đỏ.",
      "lá": "Lá kép dài tới 30 cm, có 1 – 5 lá chét mọc so le, ít khi mọc đối, hình mác thuôn, dài 6 – 16 cm, rộng 2 – 5 cm, mép nguyên hoặc có răng cưa không rõ, gốc tròn, đầu nhọn, mặt trên bóng nhẵn… mặt dưới vàng nhạt.",
      "hoa": "Cụm hoa mọc thành chùm tán ở đầu cành, ngắn hơn lá; hoa nhỏ màu trắng, nhẵn, lá đài 5, rất ngắn, hình tròn, cánh hoa thuôn nhẵn, nhị 10, có 5 cái dài gần bằng cánh hoa, bầu nhẫn có 5 ô.",
      "quả": "Quả mọng, hình cầu, khi chín màu hồng trong, ăn được.",
      "mùa hoa": "tháng 11 – 3",
      "mùa quả": "tháng 11 – 3"
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Chi Glycosmis Correa gồm các loài cây gỗ nhỏ hoặc gỗ nhỡ, phân bố chủ yếu ở vùng nhiệt đới và ẩm nhiệt đới châu Á (các nước Đông Nam Á, Nam Trung Quốc và Ấn Độ) và Australia. Ở Việt Nam có 20 loài, trong đó cơm rượu là cây có vùng phân bố phổ biến ở hầu hết các tỉnh, từ đồng bằng, trung du (tập trung nhất) đến vùng núi thấp dưới 1000 m.",
      "thường_mọc": "Cơm rượu là cây ưa sáng, ưa ẩm có thể hơi chịu bóng và khô. Cây thường mọc lẫn với các loại cây bụi khác trong các lùm bụi quanh làng, bờ nương rẫy hoặc ở đồi.",
      "Đặc điểm sinh trưởng": "Cây mọc ở vùng thấp thường xanh tốt quanh năm. Ở vùng núi cao khoảng 1000m, cây có hiện tượng rụng lá mùa đông."
    },
    {
      "@type": "Thông tin khác",
      "thong_tin_khac": ""
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Cành, lá, rễ cơm rượu thu hái quanh năm, dùng tươi hay phơi khô.."
    },
    {
      "@type": "Thành phần",
      "thành phần": "Cơm rượu chứa nhiều alcaloid : dictamin, skimiamin, kokusaginin, noracromycin, arborin, arborinin, glycosminin (Đỗ Tất Lợi, 1976), acid glycoric (CA 125: 110305k).",
      "Các hợp chất": "Hoa chứa các alcaloid : arborin, arborinin, skimiamin, glycorin, glycosmin và một amid là methylaminobenzamid (CA 125: 79349b).",
      "triết xuất": "Rễ chứa glycozolicin, 3 – formylcarbazol và glycosinin (CA 117: 147 198p)."
    },
    {
      "@type": "Tính vị",
      "Có vị": "Rễ cơm rượu có vị đắng, cay, tính ấm, có tác dụng hành ứ, hoạt huyết, tiêu sưng, giảm đau, trừ thấp."
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Rễ cơm rượu được dùng chữa phong thấp, chân tay nhức mỏi, bán thân bất toại, tích huyết tử cung ở phụ",
      "được_dùng": "Nước ép lá có vị đắng, dùng chữa sốt, bệnh về gan và trị giun.",
      "giã nát": "Một ít bột nhão từ lá cơm rượu và gừng giã nát đắp trị eczema và bệnh về da.",
      "sắc nước uống": "Nước sắc rễ chữa sưng mặt.",
      "có tác dụng": "Những cành nhỏ có sợi có tác dụng làm săn và được dùng ở một số vùng ở Ấn Độ để chải răng."
    },
    {
      "@type": "Cách dùng",
      "sắc uống": "Chữa phụ nữ kém ăn, vàng da sau khi sinh đẻ:\u00a0Lá cơm rượu sao vàng 10g. Sắc, chia 2 lần uống trong ngày.",
      "sắc đặc uống": "Chữa phụ nữ sau khi sinh đẻ bị tích huyết tử cung, ăn chậm tiêu, bụng chướng: Rễ, lá và cành cơm rượu, mỗi thứ 40g, sao qua, tán nhỏ, rồi sắc đặc uống vào lúc đói, ngày 3 lần, khi uống thêm khoảng 30 ml rượu trắng.",
      "sắc đặc uống": "Chữa phong thấp, đau nhức mình mẩy và khớp xương:\u00a0Rễ cơm rượu, rễ cốt khí, rễ cỏ xước, rễ độc lực, củ kim cang, dây đau xương, hoa kinh giới, xuyên tiêu, mỗi thứ 20g, sắc đặc uống. Nếu tay chân đau nhức, co cứng khó cử động, thêm uy linh tiên, rễ gắm, thiên niên kiện. Nhức xương nhiều, thêm rễ tầm xuân và dây cà gai leo, mỗi vị 20g.",
      "chưng lên": "Chữa mụn ổ gà mọc ở bẹn nách, thối loét lâu ngày:\u00a0Lá cơm rượu, lá ổi, lá thổ phục linh mỗi thứ một nắm.\u00a0Cả 3 vị rửa sạch, thái nhỏ, chưng lên lấy lá chuối non hơ nóng cho mềm, gói thuốc lại, to nhỏ tùy theo mụn nhọt. mặt nào định đặt lên mụn thì châm nhiều lỗ cho nước thuốc từ lá chưng nóng dễ thấm vào mụn.",
      "giã đắp": "Chữa mụn nhọt , chốc lở, rắn cắn:\u00a0Lá cơm rượu \u00a0tươi 30-40g giã đắp hoặc nấu nước tắm."
    },
    {
      "@type": "luu_y",
      "luu_y": ""
    }
  ]
