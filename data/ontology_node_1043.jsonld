  "@graph": [
    {
      "@type": "Plant",
      "ten": "Lá dứa",
      "ten_khac": "Dứa thơm, Lá dứa",
      "ten_khoa_hoc": "Pandanus amaryllifolius Roxb.",
      "ma_taxon": "123046",
      "ho": "Pandanaceae ( Dứa dại)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây mọc thành bụi, có thể cao 1m, đường kính thân 1-3cm, phân nhánh.",
      "lá": "Lá hình mũi mác, nhẵn, xếp thành hình máng, dài 40-50cm, rộng 3-4 cm, mép không gai, mặt dưới có màu nhạt, có nhiều gân cách nhau 1mm, mùi thơm như mùi cơm nếp. Để khô càng thơm hơn. Chưa thấy cây ra hoa."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Chưa thấy có tài liệu nghiên cứu. Chỉ mới biết trong lá tươi hay khô (càng khô càng thơm) có một chất thơm bền mùi, chịu được nhiệt."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Chỉ mới thấy nhân dân, đặc biệt nhân dân các tỉnh phía Nam, dùng làm thơm kẹo bánh. Chưa hề thấy ai nói gặp hiện tượng ngộ độc do dùng lá dứa này để làm thơm thức ăn. Dân gian dùng lá dứa cắt thành khúc phơi khô pha nước nóng uống như nước trà, ngoài ra lá dứa khô và cây cỏ sữa đất khô nấu chung làm nước uống để ổn định đường huyết của người bị bệnh tiểu đường loại 2 rất hiệu quả."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Cây mọc hoang dại và được trồng đặc biệt tại các tỉnh phía Nam để lấy lá tươi hay khô cho vào thức ăn như bánh, kẹo, rượu… Tại các chợ của thành phố Hồ Chí Minh người ta thường bó thành từng bó gồm từ 7-10 lá, nhiều bó nhỏ bó lại thành bó lớn. Gần như thu hái quanh năm."
    },
    {
      "@type": "luu_y"
    }
  ]
