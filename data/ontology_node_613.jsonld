  "@graph": [
    {
      "@type": "Plant",
      "ten": "Tri mẫu",
      "ten_khac": "Tri mẫu",
      "ten_khoa_hoc": "Anemarrhena asphodeloides Bunge",
      "ma_taxon": "785991",
      "ho": "Liliaceae (Hành)"
    },
    {
      "@type": "Mô tả",
      "lá": "dài khoảng 20-30cm, hẹp, đầu nhọn, phía dưới ôm vào nhau.",
      "hoa": "màu trắng",
      "cây": "Tri mẫu là một loại cỏ sống lâu năm, thân rễ chạy ngang.",
      "cao": "chừng 60-90cm."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Trong tri mẫu có một chất saponin gọi là asphonin. Ngoài ra còn một chất có tinh thể chưa xác định."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Theo tài liệu cổ tri mẫu có vị đắng, tính lạnh, không độc, có tác dụng tu thận, bổ thùy, tá hỏa, thường được dùng chữa bệnh tiêu khát (đái đường), hạ thuỷ, ích khí. Hiện nay trí mẫu thường được dùng làm thuốc chữa ho, tiêu đờm, chữa sốt, sốt do viêm phổi. Ngày dùng 4 đến 10g dưới dạng thuốc sắc."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Chữa bụng chướng to, rất cứng rắn, chân tay nhỏ, ăn uống không được: Uống thuốc gì cũng không khỏi, sau uống bài ngũ linh tán gồm các vị tri mẫu, đan sâm, độc hoạt, hải tảo, quỉ vũ tiến, tần bông (hai vị sau chưa xác định) thì thấy lợi tiểu tiện, ăn uống được bệnh dần dần khỏi (theo sách Thiên Kim ngoại đài).",
      "làm thuốc": "Chữa viêm phổi: Tri mẫu 5g, tang bạch bì l0g, mạch môn đông 8g, nước 600ml, sắc còn 200ml, chia 3 lần uống trong ngày.",
      "có tác dụng": "Dương vật cường luôn: Tri mẫu, hoàng bá, xa tiền, mộc thông, thiên môn đông, sinh thảo (cam thảo sống) các vị bằng nhau, mỗi vị 4g sắc uống.",
      "sắc uống": "Có mang động thai: Tri mẫu 80g, tán nhỏ, viên với mật bằng hạt ngô, mỗi ngày uống 20 viên, chiêu với nước cháo.",
      "giã nát": "Hắc lào: Tri mẫu mài với dấm,bôi lên."
    },
    {
      "@type": "luu_y"
    }
  ]
