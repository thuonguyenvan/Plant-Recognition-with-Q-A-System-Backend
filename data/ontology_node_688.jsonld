    {
      "@type": "Plant",
      "ten": "Thanh táo",
      "ten_khac": "Thanh táo, Thuốc trặc, Tu huýt, Tần giao, Tần lông, Tần cửu, Bơ chầm phòn (Thái)",
      "ten_khoa_hoc": "Justicia gendarussa L.",
      "ma_taxon": "347670",
      "ho": "Acanthaceae (Ô rô)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây tần cửu hay thanh táo là một cây nhỏ cao chừng 1,5m, cành có màu tím sẫm hay xanh lục, nhẵn, giữa chỗ lá mọc đối có một dòng lông.",
      "lá": "Lá mọc đối, mang cuống ngắn, phiếu lá hình mác thuôn, dài 4-14cm, rộng 1-2cm, mép nguyên. Lá thường bị loài nấm Puccinia thwaitesii ăn hại. Mặt lá nhẵn có gân xanh hay màu tím tùy theo cây.",
      "hoa": "Hoa màu trắng hay hơi điểm hồng, có những đốm tía, mọc thành bông ở đầu cành hay kẽ lá phía ngọn.",
      "quả": "Qủa nang dài 12mm, trong chứa 4 hạt.",
      "mùa hoa": "vào mùa hạ",
      "mùa quả": "vào mùa hạ"
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Trong cây thanh táo có chứa một ancaloit gọi là justixin và một lượng rất ít tinh dầu(0,001%)",
      "Các hợp chất": "Hoạt chất khác chưa rõ"
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Nhân dân thường dùng lá hay cành cây thanh táo giã đắp vào các vết sưng hay sắc nước, nước còn nóng đắp vào chỗ sưng đau, đau thấp, đau xương, có khi ngâm rượu uống chữa tê thấp",
      "có tác dụng": "hoạt huyết, trấn thống (làm cho máu lưu thông và giảm đau) còn có tác dụng lợi đại tiện, chữa da vàng (hoàng đảm), ho sốt.",
      "được_dùng": "Ngày dùng 6-12g dưới dạng thuốc sắc hay thuốc cao hoặc ngâm rượu"
    },
    {
      "@type": "Cách dùng",
      "chữa": "ho, sốt, mồ hôi trộm: Rễ thanh táo, miết giáp, địa cốt bì, sài bồ, mỗi vị 10g, đương quy, tri mẫu, mỗi vị 5g, thanh cao, ô mai, mỗi vị 4g, nước 600ml, sắc còn 200ml, chia 3 lần uống trong ngày.",
      "chữa": "vết lở, vết thương chảy máu, nhọt lở thối khó liền miệng: Lá thanh táo, lá mỏ quạ, lượng bằng nhau, rửa với nước muối, giã nhỏ, đắp rịt. Thay thuốc hằng ngày. Kết hợp uống nước sắc bạch chỉ nam, kim ngân hoa, mỗi vị một nắm và ăn rau muống hằng ngày."
    },
    {
      "@type": "luu_y"
    }
