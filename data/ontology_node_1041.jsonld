    {
      "@type": "Plant",
      "ten": "Cổ Giải",
      "ten_khac": "Cổ Giải",
      "ten_khoa_hoc": "Millettia sp .",
      "ma_taxon": null,
      "ho": "Fabaceae (Cánh bướm)",
      "description": {
        "@type": "Mô tả",
        "cây": "Cổ giải là một loại cây gỗ lớn, cao từ 10-12m, tán cây xum xuê nhiều cành, nhiều lá.",
        "thân": "Thân cây to, tròn, đường kính có thể tới 30-40cm, mọc thẳng màu nâu xanh thẫm, có nhiều nốt sần của lỗ bì thành những điểm hơi trắng bạc. vỏ cây rất dễ bóc, để lộ lớp gỗ màu trắng.",
        "lá": "lá mọc cách kép lông chim, rìa lẻ với 5-9 lá chét, đôi khi chỉ có 3 lá chét (cây nhỏ). Lá chét dài 5-7cm, rộng 2-3cm. lá chét có cuống ngắn 5-7mm.",
        "hoa": "hoa mọc thành chùm, màu trắng vàng.",
        "quả": "Quả giáp hình mã tấu, đầu nhọn. vỏ quả có lông màu vàng nâu nhạt, mịn"
      },
      "phan_bo": {
        "@type": "Phân bố"
      },
      "thong_tin_khac": {
        "@type": "Thông tin khác"
      },
      "bo_phan_dung": {
        "@type": "Bộ phận dùng"
      },
      "thanh_phan": {
        "@type": "Thành phần",
        "thành phần": "Có thể có chất rotenone. Nhưng phản ứng chưa rõ rệt"
      },
      "tinh_vi": {
        "@type": "Tính vị"
      },
      "cong_dung_y_hoc": {
        "@type": "Công dụng",
        "cong_dung_y_hoc": "Hiện nay ngoài công dụng cho gỗ, vỏ thân cây cổ giải được nhân dân nhiều nơi dùng làm thuốc diệt ruồi, cách dùng rất đơn giản chỉ cần lấy vỏ tươi hay khô (tươi có tác dụng mạnh hơn) giã nát thêm ít nước cơm hay cháo để vào nơi ruồi bay qua. Ruồi ăn phải nước cháo có chất vỏ cổ giải sẽ chết ngay tại chỗ. Để lâu vỏ bị khô, tác dụng có giảm sút."
      },
      "cach_dung": {
        "@type": "Cách dùng",
        "được_dùng": "Cổ giải là một loại cây sống ở những vùng núi đá vôi tại các tỉnh Ninh Bình, Nam Định, Hà Nam, Vĩnh Phúc, Phú Thọ, Hà Giang, Tuyên Quang. Có thấy mọc cả ở núi đất"
      },
      "luu_y": {
        "@type": "luu_y"
      }
    }
