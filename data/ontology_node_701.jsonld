    {
      "@type": "Plant",
      "ten": "Địa Liền",
      "ten_khac": "Địa liền, Sơn nại, Tam nại, Củ thiền niền, Co xá choóng (Thái), Sa khương",
      "ten_khoa_hoc": "Kaempferia galanga L.",
      "ma_taxon": "346606",
      "ho": "Zingiberaceae (Gừng)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây thảo, sống lâu năm, không có thân.",
      "rễ": "Thân rễ gồm nhiều củ nhỏ, hình trứng mọc nối tiếp nhau, có nhiều vân ngang.",
      "lá": "Lá 2-3 cái hình trứng gần tròn, xòe rộng sát mặt đất, đầu tù rồi thuôn nhọn, gốc thuôn hẹp thành một cuống ngắn rộng có rãnh, mặt trên nhẵn bóng, mặt dưới có lông mịn, mép mỏng màu đỏ, hai mặc có nhiều chấm hình tròn, phiến lá dài 8-10cm, rộng 6-7cm.",
      "hoa": "Cụm hoa không cuống, nằm ẩn trong bẹ lá; lá bắc hình mũi mác nhọn; hoa 6-12 cái, xếp thành hình bánh xe, màu trắng có đốm tím ở giữa; đài có 3 răng dài, hẹp và nhọn; tràng có ống, dài mang 3 thùy; nhị không có chỉ nhị, bao phần có 2 ô song song; có nhị lép, cánh môi to chẻ thành 2 thùy.",
      "thân": "Toàn cây nhất là thân rễ có mùi thơm và vị nóng.",
      "mùa hoa": "tháng 5-7."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Thân rễ địa liền khô chứa 2,4-3,9% tinh dầu.",
      "gồm chất": "Thành phần chủ yếu là acid p-methoxycinamic, ethylcinamat và p-methoxy ethylcinamat."
    },
    {
      "@type": "Tính vị",
      "Có vị": "Theo tài liệu cổ địa liền có vị cây, tính ôn, vào 2 kinh tỳ và vị, có tác dụng ôn trung tán hàn, trừ thấp. Tác dụng chữa ngực bụng đau lạnh, đau răng."
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Địa liền thường được dùng làm thuốc giúp sự tiêu hoá, làm cho ăn ngon, chóng tiêu và còn dùng làm thuốc xông.",
      "được_dùng": "Ngâm rượu dùng xoa bóp chữa phù tê, tê thấp nhức đầu, đau nhức.",
      "làm thuốc": "Ngày dùng 2-4g dưới dạng thuốc sắc, thuốc bột, thuốc viên hay pha chè mà uống, còn dùng trong kỹ nghệ cất tinh dầu chế nước hoa và bảo vệ quần áo chống nhậy.",
      "chữa": "Địa liền 2g, quế chi 1g, hai vị tán nhỏ chia làm 3 lần uống trong một ngày, mỗi lần 0,5 hay 1g bột. Dùng chữa ăn uống không tiêu, đau dạ dày, đau thần kinh (Diệp Quyết Tuyền)."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Cây mọc hoang và được trồng khắp nơi trong nước ta. Cây còn mọc ở Cam puchia, Trung quốc (Quảng đông, Quảng tây, Vân nam, đài loan), Malaixia, Ấn Độ.",
      "phơi khô": "Từ tháng 12-3 năm sau, người ta đào củ về, cần chọn những cây đã trên 2 năm, rửa sạch sạch đất cát, thái thành miếng mỏng, xông diêm sinh một ngày rồi phơi khô. Tuyệt đối không sấy than, củ sẽ đen mùi kém thơm.",
      "xấy khô": "Có nơi chỉ đào củ về, rửa sạch phơi khô. Địa liền rất dễ bảo quản, hầu như không bị mốc mọt mặc dù điều kiện bảo quản không hơn so với các vị thuốc khác."
    },
    {
      "@type": "luu_y"
    }
