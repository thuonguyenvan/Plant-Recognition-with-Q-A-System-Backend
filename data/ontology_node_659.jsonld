  "@graph": [
    {
      "@type": "Plant",
      "ten": "Vàng đắng",
      "ten_khac": "dây đằng giang, hoàng đằng, dây khai, vàng đắng.",
      "ten_khoa_hoc": "Coscinium fenestratum (Goetgh.) Colebr.",
      "ma_taxon": "427389",
      "ho": "Menispermaceae (Tiết dê)"
    },
    {
      "@type": "Mô tả",
      "cây": "Vàng đắng là một cây leo to, có phân nhánh, mọc bò trên mặt đất hoặc leo lên những cây gỗ cao.",
      "thân": "Thân hình trụ, đường kình từ 5-10cm. Thân non màu trắng bạc, thân già màu ngà, xù xì có vết tích của lá rụng. Cắt ngang thân có hình bánh xe với những tia tủy như nan hoa bánh xe, màu vàng, giữa có vòng lõi tủy xốp,",
      "lá": "lá mọc so le, mặt trên xanh, mặt dưới màu trắng nhạt, dài 15-30cm, rộng 10-20cm, có 5 gân (3 gân nỗi rõ). Mặt dưới có phủ lông tơ.",
      "hoa": "Hoa màu trắng phớt tím, mọc thành xim ở kẽ lá. Cuống hoa rất ngắn,",
      "rễ": "rễ hình trụ, đầu thuôn hình nón, mặt ngoài màu trắng nhạt, mặ trong màu vàng, cắt ngang có hình bánh xe với những tia tủy hình nan hoa.",
      "mặt": "Vị đắng."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Trong vàng đằng có nhiều ancaloit dẫn xuất của izoquinolein, chủ yếu là berberin. Tỷ lệ berberin chiếm từ 1,5-2-3% (Nguyễn Liêm và cộng sự, 1975)"
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Nhân dân những vùng có cây hoàng đằng mọc hoang dại thường dùng thân và rễ cây này để nhụôm màu vàng và dùng làm thuốc như vị hoàng đằng làm thuốc chữa sốt, sốt rét, lỵ, đau mắt. Dùng dưới hình thức bột hay viên. Ngày uống 4-6g Có thể dùng làm nguyên liệu chiết berberin. Berberin clorua có thể chữa sốt, sốt rét, lỵ, đau mắt, dùng trong: ngày uống 0,02g – 0,2g dưới dạng thuốc viên. Người ta còn dùng chữa bệnh về gan, mật, vàng da, ăn uống khó tiêu. Dùng ngoài: chế thuốc đau mắt dưới dạng dung dịch 0,5%-1%."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Mọc hoang dại rất phổ biến ở miền Đông Nam Bộ, Nam Trung Bộ, Tây Nguyên. Còn thấy mọc nhiều ở Trung và Hạ Lào, Campuchia. Trữ lượng khá nhiều, người ta dùng thân và rễ, thu hái hầu như quanh năm. Hái về thái mỏng, phơi hay sấy khô, không phải chế biến gì khác."
    },
    {
      "@type": "luu_y"
    }
  ]
