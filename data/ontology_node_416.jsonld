    {
      "@type": "Plant",
      "ten": "Cơm cháy tròn",
      "ten_khac": "Mậu mạ",
      "ten_khoa_hoc": "Sambucus simpsonii Rehd.",
      "ma_taxon": "84300",
      "ho": "Cơm cháy (Caprifoliaceae)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây bụi, cao 3 – 4m.",
      "thân": "Thân có lõi to, xốp.",
      "lá": "Lá kép lông chim, mọc đối, gồm 7 – 13 lá chét hình trứng nhọn, mép khía răng, mặt trên nhẵn màu lục bóng, mặt dưới nhạt, lá chét ở gốc thường xẻ thêm hai lần, ở cành mang hoa, số lá chét ít hơn (3 – 7 lá).",
      "hoa": "Cụm hoa mọc ở đầu cành thành ngủ phân nhánh, có ít lông; hoa nhiều, đường kính 4 – 5 mm, có mùi đặc biệt; đài nhỏ, hình đấu, 5 răng màu trắng tràng có 5 cánh tròn, màu vàng ngà; nhị 5 có bao phấn màu vàng, thuôn dài; bầu có 3-5 ô, núm nhụy chia 5 thuỳ.",
      "quả": "Quả mọng hình cầu, đường kính 4-5 mm, khi chín màu không sau tím đen, thịt quả có màu xám vị ngọt, hạt 4 – 5, hình thuôn hay ellip.",
      "mùa hoa": "gần như quanh năm.",
      "mùa quả": "gần như quanh năm."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Chi Sambucus ở Việt Nam có 2 loài, phân bố rộng ở nhiều tỉnh miền núi phía bắc. Trong đó loài Cơm cháy tròn mới ghi nhận được ở Thừa Thiên Huế (Bạch Mã) và Lâm Đồng (Đà Lạt), cây còn có ở Lào. Cơm cháy tròn là loại cây ưa ẩm, ưa sáng.",
      "thường_mọc": "thường mọc rải rác ở chỗ đất trống ven rừng hoặc gần các nguồn nước."
    },
    {
      "@type": "Thông tin khác",
      "thong_tin_khac": null
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Toàn cây."
    },
    {
      "@type": "Thành phần",
      "thành phần": "Cơm cháy tròn chứa các nhóm chất chủ yếu sau đây glycosid cyanogeric: satimbunigrin; các anthocyanin, các glycosid iridoid: morronisid.",
      "Các hợp chất": "Các chất triterpen, acid ursolic, acid oleanolic, tinh dầu, protein, đặc biệt là các protein làm bất hoạt ribosom của tế bào (RIPs)."
    },
    {
      "@type": "Tính vị",
      "Có vị": "Lá và toàn cây cơm cháy tròn vị đắng, tính ấm, có độc, có công năng chỉ huyết, sinh cơ, sát trùng."
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Lá và ngọn cây cơm cháy thường được dùng ngoài, rửa sạch, giã nát đắp lên các chỗ viêm nhiễm, mụn nhọt, nấm ngoài da hoặc đắp lên chỗ bầm tím, các vết chém chặt để cầm máu và làm vết thương chóng lành.",
      "được_dùng": "Ở Mỹ, thổ dân thường dùng vỏ trong của cây làm chè để lợi tiểu, nhuận tràng, gây trung tiện và làm ra mồ hôi.",
      "sắc nước uống": "Nước sắc vỏ cây được dùng rửa để chữa eczema, lở loét hoặc rộp da.",
      "giã nát": "Lá và ngọn cây giã nát đắp lên chỗ bầm tím, vết chém chặt để cầm máu, và chóng lành vết thương, đắp lên chỗ sưng."
    },
