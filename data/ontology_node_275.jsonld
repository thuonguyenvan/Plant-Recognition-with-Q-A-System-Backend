    {
      "@type": "Plant",
      "ten": "Ngâu rừng",
      "ten_khac": "Ngâu rừng, Ngâu cánh",
      "ten_khoa_hoc": "Aglaia pleuropteris Pierre",
      "ma_taxon": "189063",
      "ho": "Meliaceae (Xoan)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây nhỏ, cao 2- 6m.",
      "thân": "Cành non mềm, có ít lông, vỏ màu xám nâu.",
      "lá": "Lá kép lông chùm lẻ, mọc so le gồm 11 – 15 lá chét thuôn – mũi mác, dài 2 – 3,5 cm, rộng 1 – 1,5cm, gốc tròn, đầu nhọn, gân lá men theo mép, nổi rõ ở hai mặt; cuống chung dài 6-12cm, có cánh.",
      "hoa": "Cụm hoa mọc ở kẽ lá thành chùm, ngắn hơn lá; hoa nhỏ màu vàng, thơm, thường đơn tính do khuyết bầu; lá bắc ngắn, có lông; đài nhẵn, ngắn hơn tràng, 5 răng có lông ở mép; tràng 5 cánh nhẵn, khum; nhị 5, bao phấn có 4 ô; bầu rất nhỏ, có lông.",
      "quả": "Quả hình trứng, nhẵn, có một hạt, dài 1 – 1,2cm; hạt màu nâu.",
      "mùa hoa": "tháng 12-2",
      "mùa quả": "tháng 12-2",
      "mọc": "Tránh nhầm với loài ngâu dại, có 5 lá chét to; hoa cũng vàng thơm, rất giống loài trên."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Aglaia Lour. là một chi lớn khoảng 39 loài ở Việt Nam (Nguyễn Tiến Bân, 1997). Loài ngâu rừng hiện chưa rõ nguồn gốc phát sinh, song về phân bố đã ghi nhận được ở Đông Dương, Thái Lan và có thể cả một số nước khác trong vùng Đông – Nam Á. Ở Việt Nam, ngâu rừng phân bố chủ yếu ở các tỉnh phía nam.",
      "thường_mọc": "Cây ưa sáng, có thể hơi chịu bóng, thường mọc ở kiểu rừng thưa, rừng thứ sinh và đôi khi ở ven rừng nguyên sinh ẩm. Cây ra hoa quả nhiều hàng năm, nhất là ở những cây được chiếu sáng nhiều, tái sinh tự nhiên chủ yếu từ hạt và sau khi chặt có khả năng tái sinh cây chồi khỏe."
    },
