  "@graph": [
    {
      "@type": "Plant",
      "ten": "Cây khổ sâm cho rễ",
      "ten_khac": "Khổ sâm, Khổ sâm Trung Quốc",
      "ten_khoa_hoc": "Sophora flavescens Ait.",
      "ma_taxon": "138582",
      "ho": "Fabaceae (Đậu)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây nhỏ cao 0,5-1,2m.",
      "rễ": "Rễ hình trụ, vỏ ngoài màu vàng",
      "lá": "lá kép lông chim lẻ, mọc so le, gồm 5-10 đôi lá chét. Lá chét hình mác dài 2-4,5cm, rộng 7-16mm.",
      "hoa": "Hoa mọc thành chùm dài 10- 20cm. Hoa màu vàng trắng.",
      "quả": "Quả giáp dài 5-12cm, đường kính 5-8mm, đầu có mỏ dài chứa 3-7 hạt, gần hình cầu, màu đen."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Trong rễ khổ sâm, người ta đã nghiên cứu thấy các chất như: 2,5% chất ancaloid có tên matrin C15H24N2O, sophocacpin C15H24ON2.H2O, oxymatrin C15H24ON2.H2O . Ngoài ra trong rễ còn có chất xytisin C11H14ON2- Trong lá khổ sâm có chứa 47mg% vitamin C. Trong hoa có 0,12% tinh dầu. Chất matrin được nhà bác học Nhật Bản Nagai chiết xuất từ năm 1899",
      "gồm chất": "Matrin có nhiều dạng tinh thể và nhiều độ chảy khác nhau 76-870 và 840. Hơi tan trong ête, cồn, clorofooc, benzen, nước lạnh, ít tan trong ête dầu hoả, ít tan trong nước nóng hơn trong nước lạnh."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Khổ sâm là một vị thuốc bổ đắng, thuốc lợi tiểu tiện. Ngoài ra còn công dụng chữa lỵ, sốt quá hoá điên cuồng, còn dùng chữa giun và ký sinh của súc vật, sắc nước rửa mụn nhọt, lở loét.",
      "được_dùng": "Ngày dùng 12g dưới dạng thuốc sắc, chia làm 2-3 lần uống trong ngày.",
      "chữa": "Chữa sốt quá hoá điên cuồng: Khổ sâm tán bột, thêm mật vào viên thành viên bằng hạt ngô. Ngày uống 10 viên. Dùng nước sắc bạc hà làm thang chiêu thuốc. Thuốc chữa lỵ: Khổ sâm sao vàng tán nhỏ. Ngày uống 3 lần, chữa lỵ ra máu."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Cây mọc ở một số tỉnh phía Bắc Việt Nam. Hiện vị thuốc vẫn nhập của Trung Quốc. Tại Trung Quốc cây mọc ở khắp các tỉnh Vân Nam, Phúc Kiến, Quảng Đông, Hà Bắc.",
      "phơi khô": "Thu hái củ, rửa sạch, thái lát, phơi khô; hoặc đem củ tươi ngâm nước vo gạo nếp một đêm, rửa sạch, để trong 3 giờ, rồi mới thái lát, phơi khô.",
      "luu_y": "Bảo quản nơi khô ráo."
    },
    {
      "@type": "luu_y"
    }
