  "@graph": [
    {
      "@type": "Plant",
      "ten": "Lúa",
      "ten_khac": "Lúa tẻ, Ngạnh mễ, Khẩu chăm (Tày), Ba té (Bana), Còi (Kho)",
      "ten_khoa_hoc": "Oryza sativa L.",
      "ma_taxon": "61381",
      "ho": "Poaceae (Lúa)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cỏ mọc hằng năm, cao 0,7-1,5m.",
      "lá": "Lá có phiến dài, bìa ráp, bẹ cao, trắng, lưỡi bẹ có lông.",
      "hoa": "Chuỳ bao gồm nhiều bông, mang các bông nhỏ màu vàng vàng. Mày hoa có lông gai, 1 hoa, 6 nhị.",
      "quả": "Quả thóc dính chặt với mày hoa (trấu), ta quen gọi là hạt lúa."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Cây của vùng Á châu nhiệt đới, được trồng làm lương thực ở khắp nơi. Có hai thứ là Lúa tẻ – var.utilissima A. Camus và Lúa nếp – var. glutinosa Tanaka. Thu hái các bộ phận của cây quanh năm."
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Hạt thóc, rễ lúa – Semen et Radix Oryzae; đạo nha – Fructus Oryzae Germunatus."
    },
    {
      "@type": "Thành phần",
      "thành phần": "Người ta cũng đã biết trong lúa có các thành phần sau: Vitamin A, B, D và E, mỡ 20%, hydratcarbon, protein, adenin, cholin, acid arachidic, lignoxeric, palmitic, aloic, phytosterin."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Gạo là thành phần quan trọng bữa ăn của nhân dân ta, bồi bổ cho cơ thể và đem lại cân bằng cho cơ thể. Hạt thóc đã ngâm cho nẩy mầm rồi phơi khô gọi là cốc nha, đạo nha dùng thay cho mạch nha, giúp sự tiêu hoá và là thức ăn có tinh bột có tác dụng rất tốt cho những người ăn uống kém tiêu, không muốn ăn, còn chữa các bệnh phù do thiếu vitamin. Gạo lâu năm dùng trị đau bụng và trị lỵ. Gạo nếp dùng trị các chứng đau bụng, nôn mửa và tiểu tiện ra chất nhờn (dưỡng trấp). Ở Philippin, cám được dùng để chế thuốc phòng và chữa bệnh thiếu các loại vitamin B. Dầu cám dùng trộn với rau để ăn. Rễ và thân của Lúa là thuốc lợi tiểu. Quả thóc chưa bóc vỏ dùng làm thuốc đắp cho dịu.",
      "chữa": "Giải phiền nhiệt trong trường hợp sốt cao, ra nhiều mồ hôi, háo khát: Dùng Gạo tẻ một nắm, lá Tre hay Cỏ lá tre một nắm cùng sắc uống. Có thể thêm bột thạch cao 10-12g cùng uống.",
      "làm thuốc": "Nôn mửa hay ỉa chảy háo khát, rối loạn tiêu hoá: Dùng Gạo tẻ sao sắc uống thay nước và thay ăn.",
      "sắc uống": "Chữa nôn ói không dứt: Dùng Gạo nếp 20g, sao vàng, Gừng củ 3 lát, sắc uống (Nam dược thần hiệu)."
    },
    {
      "@type": "Cách dùng"
    },
    {
      "@type": "luu_y"
    }
