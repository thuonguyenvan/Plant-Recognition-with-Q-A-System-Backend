    {
      "@type": "Plant",
      "ten": "Đại",
      "ten_khac": "Đại hoa đỏ, Sứ cùi, đại",
      "ten_khoa_hoc": "Plumeria acutifolia Poir.",
      "ma_taxon": "314493",
      "ho": "Apocynaceae (Trúc đào)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây đại là một cây nhỡ, cao 3-7m, cành mẫm, to.",
      "lá": "Lá mọc so le sít nhau, thường tập trung ở đầu cành; lá dày, nguyên, dài 15-35cm, rộng 5-10cm hai đầu đều hẹp nhọn, mặt nhẵn bóng, gân giữa nổi rõ.",
      "hoa": "Hoa màu trắng, rất thơm, mặt trong ở phía dưới màu vàng dài 4-5cm.",
      "quả": "Quả đại dài hình trụ, dài 12cm.",
      "mùa hoa": "tháng 4-8"
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Trong vỏ thân, Peckolt và Geuther đã tìm thấy một glucozit gọi là agoniađin C10H14O6 có tinh thể hình kim mềm, chảy ở 155°C, ít tan trong nước, trong rượu, trong sunfua cacbon, ête và benáa, tan trong axit nitric và sunfuric. Dung dịch màu vàng tươi, nhưng dần dần ngả màu xanh lục. Dưới tác dụng của axit loãng và đun sôi, agoniađin sẽ cho glucoza và một phần chưa xác định được.",
      "triết xuất": "Oudman còn chiết được từ nhựa cây một axit gọi là axit plumeric C10H10O5, có tinh thể hình kim nhỏ, tan trong nước sôi, rượu và ête, chảy và bị phân tích ở 130°C.",
      "đã nghiên cứu": "Năm 1952, Grumbach A., Schmiđ H. và Bencze w. (1952, Uberein Pfianzliches Antibioticum. Experimentia, Suisse, 8, (6): 224-225) đã chiết được từ cây hoa đại một chất kháng sinh mới đặt tên là funvoplumierin có tác dụng ức chế sự lớn lên của một số giống Mycobacterium tuberculosis.",
      "lấy từ": "G. H. Mahran (1974, Planta Medica, 5: 226) đã lấy từ rễ, lá và vỏ đại một chất đắng gọi là plumierit, một glucozit. Không có trong hoa. Plumierit là một chất bột trắng, có tinh thể, không mùi, vị đắng, độ chảy 155-156°C tan trong nước, trong cồn etylic, metylic, etylaxetat. ít tan trong ête, clorofoc, không tan trong ête dầu hoả.",
      "tinh dầu chứa": "Trong hoa có một ít tinh dầu mùi thơm mát."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Vỏ thân và vỏ rễ dùng dưới hình thức thuốc sắc 5-10g vỏ trong 200ml, có tác dụng tẩy mạnh, còn có tác dụng tháo nước (hydragogue). Dùng trong những trường hợp thũng nước (hydropisie).",
      "dùng nhựa mủ": "Có thể ,dùng nhựa mủ của thân với liều 0,50-0,80g dưới dạng nhũ dịch.",
      "chế vỏ cây": "Có khi người ta còn chế vỏ cây thành cao và dùng với liều 0,20-0,50g một ngày, tăngdần lên tới l-2g.",
      "hoa đại dùng": "Hoa đại dùng làm thuốc chữa ho, có thể dùng làm thuốc uống chữa bệnh huyết hữu (hémophilie) với liều 6-12g một ngày, dưới dạng thuốc sắc với 200ml nước, chia làm nhiều lần uống trong ngày,",
      "lá cây hoa đại": "Lá cây hoa đại giã dán và đắp vào những nơi sai khớp, bong gân, mụn nhọt.",
      "nhựa dùng": "Nhựa dùng như vỏ thân, nhưng với liều thấp hơn, còn dùng chữa trai chân và vết loét."
    },
    {
      "@type": "Cách dùng",
      "được_dùng": "Cây rất hay được trồng làm cảnh quanh chùa đền và các công viên vì dáng đẹp, hoa thơm. Một số bộ phận được dùng làm thuốc.",
      "vỏ thân": "Vỏ thân đẽo về sao vàng mà dùng, có khi phơi khô để dành, vỏ rễ cũng dùng như vỏ thân.",
      "hoa đại hái": "Hoa đại hái về, phơi khô.",
      "ngoài ra": "Ngoài ra còn dùng nụ hoa và lá tươi."
    },
