  "@graph": [
    {
      "@type": "Plant",
      "ten": "Khúc khắc",
      "ten_khac": "Khúc khắc, Dây kim cang, Dây nâu, Củ cun, Kim cang mỡ",
      "ten_khoa_hoc": "Heterosmilax gaudichaudiana (Kunth) Maxim.",
      "ma_taxon": "1477361",
      "ho": "Smilacaceae (Khúc khắc)"
    },
    {
      "@type": "Mô tả",
      "cây": "Dây leo có thân nhẵn, không có gai.",
      "lá": "Lá mọc so le, hình trứng, đôi khi gần tròn, gốc tròn hay hơi hình tim, có 6 gân gốc, cuống dài mang tua cuốn.",
      "hoa": "Cụm hoa hình tán đơn, mọc ở nách lá, có cuống dài. Hoa màu hồng hoặc có điểm chấm đỏ, hoa đực có bao hoa với 3 răng tù, 3 nhị có chỉ nhị ngắn. Hoa cái có bầu hình trứng với vòi ngắn mang 3 đầu nhụy rẽ ra.",
      "quả": "Quả mọng hình cầu hoặc có bốn góc, khi chín màu đen, chứa 2-4 hạt màu đỏ nâu.",
      "mùa hoa": "tháng 5-6",
      "mùa quả": "tháng 8-12."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Cây mọc nhiều ở vùng rừng núi các tỉnh như Quảng Ninh, Nam Hà, Ninh Bình và một số tỉnh ven biển miền Trung, thường gặp ở ven đường trong các bờ bụi, trên các đồi trọc. Khúc khắc là cây ưa sáng, có thể chịu hạn và mọc được trên nhiều loại đất, thường tập trung ở các vùng đồi cây bụi, bờ nương rẫy, nhất là trong các trảng cây bụi vùng đồi thấp ven biển. Ở một số tỉnh đồng bằng Bắc Bộ, khúc khắc có thể mọc lẫn với các cây khác trong các lùm bụi quanh làng. Cây ra hoa quả nhiều hàng năm, tái sinh tự nhiên chủ yếu từ hạt. Cây chịu được sự chặt phá nhiều lần, thậm chí có thể tồn tại sau các đợt cháy rừng, vì có thân rễ nằm sâu dưới mặt đất."
    },
    {
      "@type": "Thông tin khác",
      "thong_tin_khac": ""
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Thân rễ. Ta thu hái rễ củ vào mùa đông, mang về rửa sạch rồi phơi hay sấy khô. Củ to có thể bổ đôi."
    },
    {
      "@type": "Thành phần",
      "thành phần": "Thân rễ chứa saponin, tanin, chất nhựa."
    },
    {
      "@type": "Tính vị",
      "Có vị": "Khúc khắc có vị hơi ngọt, chát, tính bình, có tác dụng chống viêm, tiêu độc, chống dị ứng, lợi thấp, mạnh gân cốt, thanh nhiệt."
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Dùng chữa thấp khớp đau lưng, đau xương, đau khớp. Cũng dùng chữa mụn nhọt, tràng nhạc, lở ngứa, giang mai, ngộ độc thủy ngân. Ngày dùng 15-30g, dạng thuốc sắc, cao nước hoặc thuốc bột, thuốc viên. Trong thực tế, các lương y ở Việt Nam vẫn cho rằng tác dụng của khúc khắc tương tự như tác dụng của thổ phục linh nên thường dùng để thay thế."
    },
