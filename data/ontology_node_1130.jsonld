  "@graph": [
    {
      "@type": "Plant",
      "ten": "Kha tử",
      "ten_khac": "Chiêu liêu, Chiêu liêu đồng, Kha tử",
      "ten_khoa_hoc": "Terminalia chebula Retz",
      "ma_taxon": "68671",
      "ho": "Combretaceae (Bàng)"
    },
    {
      "@type": "Mô tả",
      "cây": "Chiều liêu là một cây to cao chừng 15-20m, có vỏ màu đen nhạt trên có những vạch nứt dọc.",
      "lá": "Lá mọc đối cuống rất ngắn, hình trứng, phía cuống tròn hơi thon, đầu nhọn, dài chừng 15-20cm, rộng 7-15cm, dai, hơi có lông mềm trên cả hai mặt, sau thì nhẵn, ở đầu cuống có hai tuyến nhỏ hình mắt cua.",
      "hoa": "Hoa mọc thành bông, nhỏ, màu trắng, lưỡng tính, mùi thơm, mọc ờ đầu cành hay ở kẽ lá, cuống ngắn, trên có phủ lông màu vàng nhạt.",
      "quả": "Quả hình trứng thon, dài 3^tcm, rộng 22-25mm, hai đầu tù, không có dìa, có 5 canh dọc, màu nâu vàng nhạt, thịt đen nhạt, khô, cứng và chắc. Hạch cứng, hơi hình 5 cạnh, dày chừng 10-15mm, 1 hạt, lá mầm cuốn.",
      "mọc": "Có một loại chiều liêu xanh (Terminalia citrina Roxb. hay Myrobalanus citrim Gaertn.) có lá dài hơn, nhẵn, kể cả khi còn non, quả thuôn và nhỏ hơn, nhân mỏng hơm, hạt hẹp hơn. Chiều liêu xanh mọc ở Biên Hòa."
    },
    {
      "@type": "Phân bố"
    },
    {
      "@type": "Thông tin khác"
    },
    {
      "@type": "Bộ phận dùng"
    },
    {
      "@type": "Thành phần",
      "thành phần": "Trong kha tử có tới 20-40% tanin bao gồm axit elagic, axit galic, và axit luteolic. Lượng tanin có khi lên tói 51,3% nếu quả thật khổ. Ngoài ra còn có axit chebulinic C41H34O27 với tỷ lệ 3-4%.",
      "thủy phân": "Thủy phân axit chebulinic sẽ cho một phân tử glucoza 3 phân tử axic galic và một phân tử axit có công thức C14H12O11.",
      "chứa": "Trong nhân còn có 36,7% dầu vàng nhạt, trong, nửa khô."
    },
    {
      "@type": "Tính vị"
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Kha tử là một vị thuốc chuyên dùng chữa đi ỉa lỏng lâu ngày, chữa lỵ kinh niên, còn dùng chữa ho mất tiếng, di tinh, mổ hôi trộm, trĩ, lòi dom, xích bạch đới.",
      "được_dùng": "Còn dùng trong kỹ nghệ thuộc da.",
      "sắc uống": "Ngày uống 3-6g dưới dạng thuốc sắc, thuốc viên.",
      "chữa": "Chữa xích bạch lỵ: Kha tử 12 quả, 6 quả để sống, 6 quả nướng bỏ hạt, sao vàng và tán nhỏ. Nếu lỵ ra máu thì dùng nước sắc cam thảo mà chiêu thuốc, nếu lỵ ra mũi không, dùng nước sắc cam thảo trích. Chữa ho lâu ngày: Kha tử 4g, đảng sâm 4g, sắc với 400ml nước cô đặc còn 200ml, chia 3 lẫn uống trong ngày."
    },
    {
      "@type": "Cách dùng",
      "phơi khô": "Vào tháng 9-10-11 quả chín, hái vể phơi khô.",
      "được_dùng": "Cây kha tử hay chiều liêu chỉ mới thấy mọc ở miền Nam, Campuchia (còn gọi là Sramar), Lào, Ấn Độ, Miến Điện và Thái Lan. Trước đây Trung Quốc cũng nhập cùa Ấn Độ, hiện nay có thể tự túc. Trồng ở  Vân Nam, Quảng Đông, Quảng Tây."
    },
    {
      "@type": "luu_y"
    }
  ]
