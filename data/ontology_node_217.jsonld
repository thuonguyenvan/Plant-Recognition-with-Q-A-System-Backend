    {
      "@type": "Plant",
      "ten": "Dây xanh",
      "ten_khac": "Dây xanh",
      "ten_khoa_hoc": "Tiliacora triandra (Colebr.) Diels",
      "ma_taxon": "892772",
      "ho": "Menispermaceae (Tiết dê)"
    },
    {
      "@type": "Mô tả",
      "cây": "Dây leo gỗ mọc quấn, rụng lá, dài tới 3m.",
      "thân": "Thân mảnh, hình trụ, có lông và khía mờ.",
      "lá": "Lá mọc so le, hình trái xoan, dài 3-6cm, rộng 1-3cm, có lông, nhất là ở mặt dưới; gân gốc 3-5.",
      "hoa": "Hoa nhỏ màu vàng trắng mọc thành chùm ở nách lá.",
      "quả": "Quả hạch, đen lam khi chín, to 5-6mm.",
      "mùa hoa": "tháng 5-6",
      "mùa quả": "tháng 5-6"
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Cây của vùng lục địa á châu, phân bố ở Nam Trung Quốc, Nhật Bản. Chưa rõ sự phân bố ở nước ta, chỉ gặp rất rải rác ở các tỉnh trung du hoặc núi thấp dưới 600m ở miền Bắc. Chưa thấy có ở miền Nam.",
      "thường_mọc": "Dây xanh là loại dây leo nhỏ, thường mọc lẫn với một số cây bụi hoặc cỏ cao ở ven rừng, chân đồi, bờ nương rẫy.",
      "các_tỉnh": {
        "@type": "schema:ItemList",
        "mục": [
          {
            "@type": "schema:ListItem",
            "tỉnh": {
              "@type": "schema:Place",
              "tên": "Phú Thọ"
            }
          },
          {
            "@type": "schema:ListItem",
            "tỉnh": {
              "@type": "schema:Place",
              "tên": "Vĩnh Phúc"
            }
          },
          {
            "@type": "schema:ListItem",
            "tỉnh": {
              "@type": "schema:Place",
              "tên": "Thái Nguyên"
            }
          },
          {
            "@type": "schema:ListItem",
            "tỉnh": {
              "@type": "schema:Place",
              "tên": "Lạng Sơn"
            }
          }
        ]
      },
      "Đặc điểm sinh trưởng": "Dây xanh ra hoa quả hàng năm. Chưa thấy cây con nảy mầm từ hạt. Khi bị chặt phá, phần còn lại có khả năng tái sinh."
    },
    {
      "@type": "Thông tin khác",
      "thong_tin_khac": null
    },
    {
      "@type": "Bộ phận dùng",
      "bo_phan_dung": "Rễ thu hái vào mùa hè và mùa thu. Rửa sạch, thái lát mỏng, dùng tươi hoặc phơi khô dùng dần."
    },
    {
      "@type": "Thành phần",
      "thành phần": "Trong rễ có magnoflorin, trilobin, isotrilobin, homotrilobin, trilobamin, normenisarin và colobin."
    },
    {
      "@type": "Tính vị",
      "Có vị": "Rễ dây xanh có vị đắng, tính hàn, vào hai kinh phế, bàng quang, có tác dụng trừ thấp, giảm đau, thanh nhiệt, lợi tiểu, tiêu sưng, tiêu viêm, hạ huyết áp."
    },
    {
      "@type": "Công dụng",
      "cong_dung_y_hoc": "Rễ được dùng trị  Thấp khớp, đau khớp và đau xương; Đau dạ dày, đau bụng, đau bụng kinh; Đau họng; Viêm thận phù thũng, bệnh đường tiết niệu; Thấp khớp tim; Huyết áp cao. Cũng được dùng chữa đau thần kinh.",
      "được_dùng": "Liều dùng 15-30g, dạng thuốc sắc. Dùng ngoài trị đinh nhọt, bệnh ngoài da và rắn cắn, giã rễ tươi và đắp.",
      "làm thuốc": "Ở Trung Quốc, người ta cho biết rễ chứa nhiều tinh bột, có thể dùng nấu rượu.",
      "chữa": "Thấp khớp đau nhức xương, dùng Mộc phòng kỷ, Ngưu tất, mỗi vị 15g, sắc uống. Viêm thận phù thũng, bệnh đường tiết niệu: Mộc phòng kỷ 15g, Mã đề 30g, sắc uống. Đau họng: Mộc phòng kỷ 15-30g, sắc uống."
    },
