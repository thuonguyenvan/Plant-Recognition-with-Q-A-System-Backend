    {
      "@type": "Plant",
      "ten": "Thông Đất",
      "ten_khac": "Thông đất, Thạch tùng nghiêng",
      "ten_khoa_hoc": "Lycopodiella cernua (L.) Pic.",
      "ma_taxon": "82400",
      "ho": "Lycopodiaceae (Thông đất)"
    },
    {
      "@type": "Mô tả",
      "cây": "Cây thảo, sống lâu năm, mọc ở đất, cao 30-50cm.",
      "thân": "Thân hóa gỗ hình trụ, hơi có rãnh, lúc đầu mọc bò ngang, bén rễ ở những mấu, sau mọc đứng và phân cành.",
      "lá": "Lá nhỏ, hình dải, hướng lên, xếp theo đường xoắn ốc, chỉ có 1 gân.",
      "hoa": "Bông hình trụ, màu nâu nhạt, mọc thõng xuống ở đầu cành, mang nhiều lá bao tử hình tam giác, đầu nhọn; túi bào tử nằm ở kẽ lá bào tử, gần hình cầu, mở thành hai mảnh không bằng nhau; bào tử rất nhỏ.",
      "mùa hoa": "tháng 3-7."
    },
    {
      "@type": "Phân bố",
      "phân_bố": "Chi Lycopodium L. gồm các loài là thân thảo, phân bố rải rác từ vùng ôn đới ấm đến vùng cận nhiệt đới và nhiệt đới. Ở Việt Nam gầm 10 loài.  Trên thế giới, thông đất chỉ thấy ở 1 số nước châu Á như Trung Quốc, Mianma, Lào, Ấn Độ, Việt Nam,… Ở Việt Nam, thông đất phân bố ở các tỉnh miền núi và trung du, cây thường mọc lẫn với các loại cây bụi và cỏ thấp trong các quần hệ thực vật ở đồi, ven rừng, bờ nương rãy, ven đường đi.  Thông đất là cây chịu hạn tốt, có thể mọc được trên nhiều loại đất, kể cả đất chua, khô cằn sỏi đá. Với khả năng mọc chồi khỏe mạnh từ thân rễ, cây dễ dàng tạo thành đám lớn, có tác dụng chống xói mòn.  Thông đất sinh sản bằng bào tử, bào tử phát tán nhờ gió và nước.",
      "các_tỉnh": {
        "@type": "schema:ItemList",
        "mục": [
          {
            "@type": "schema:ListItem",
            "tỉnh": {
              "@type": "schema:Place",
              "tên": "các tỉnh miền núi và trung du"
            }
          }
        ]
      }
    },
    {
      "@type": "Thông tin khác",
      "thong_tin_khac": ""
