  "@graph": [
    {
      "@type": "Plant",
      "schema:url": "https://tracuuduoclieu.vn/da-da.html",
      "ten": "Đa đa",
      "ten_khac": "Đa đa, Dây hải sơn, Xâm, Dây săng, Cò cưa, Loong cơ đa (Bana)",
      "ten_khoa_hoc": "Harrisonia perforata (Blanco) Merr.",
      "ma_taxon": "899780",
      "ho": "Simaroubaceae (Thanh thất)",
      "description": {
        "@type": "Mô tả",
        "cây": "Cây bụi, phủ lông mịn như len.",
        "thân": "Thân cành có gai cong, cành non màu nâu tím.",
        "lá": "Lá kép mọc so le, 5–11 lá chét hình trứng, nguyên hoặc khía răng, hai mặt nhẵn, mặt trên bóng màu lục sẫm, mặt dưới nhạt; cuống chung có cánh giữa các lá chét.",
        "hoa": "Cụm hoa mọc ở đầu cành hoặc kẽ lá thành xim; hoa màu trắng; đài gồm 5 thuỳ nhỏ có lông, tràng có 5 cánh thuôn; nhị 10; bầu có 5 thuỳ.",
        "quả": "Quả hạch, hình cầu, hơi dẹt, thịt màu đỏ đen; hạt cứng."
      },
      "distribution": {
        "@type": "Phân bố",
        "phân_bố": "Chi Harrisonia R.Br. chỉ có một loài đa đa ở Việt Nam. Cây cũng phân bố khá phổ biến ở vùng nhiệt đới Đông Nam Á, bao gồm Lào, Campuchia, Thái Lan, Malaysia, Philippin và Indonesia. Ở Việt Nam, vùng phân bố của đa đa từ Sơn La đến Kiên Giang và đảo Phú Quốc. Một số tỉnh ở Tây Nguyên, Đông và Tây Nam Bộ như Kon Tum, Gia Lai, Đắk Lắk, Quảng Nam, Bình Định, Phú Yên, Khánh Hòa… là nơi có cây mọc tương đối tập trung.",
        "thường_mọc": "Đa đa thuộc loại cây bụi mọc ưa ẩm, ưa sáng và có thể chịu được hạn, thường mọc thành quần thể lớn ở nơi có ánh sáng đầy đủ."
      },
      "additionalInformation": {
        "@type": "Thông tin khác",
        "thong_tin_khac": ""
      },
      "usedParts": {
        "@type": "Bộ phận dùng",
        "bo_phan_dung": "Vỏ thân, thu hái quanh năm, dùng tươi hoặc phơi sấy khô. Rễ, cành, lá đôi khi cũng được dùng."
      },
      "compositionList": {
        "@type": "Thành phần",
        "thành phần": "Từ dịch chiết n-butanol và methylen clorid của cây đa đa, bằng phương pháp kết hợp sắc ký cột trên silica gel và dianion HP-20 và sắc ký chế huyền, người ta đã tách được các chromon từ perforatin C (1), D (2), E(3), F(4) và perforatin G(5), heteroepoxerin-7-methyl ether (6); heteroepoxerin-5-methoxy-7-methyl ether (7); 2-hydroxymethylallopatavarin-5-methyl ether (8) & perforatin A (9); acid perforatic (10) và acid perforatic methyl ester (methyl ester perforatic acid) (11); các chất coumarin scopotetin, esculetin, xanthotoxin, metyl phenyl propanoat, và confeyl aldehyd (Phytochemistry 1995 40(6):1787–90).",
        "Các hợp chất": "Các hợp chất chromon pecuennin 7-methyl ether, O-methyl allopatavarin và acid perforlic cũng đã được phân lập từ cây đa đa (CA. 1995, 123, 251328 m).",
        "tìm thấy": "Trong cành, người ta cũng tìm thấy các quassinoid perforaquassin A, B, C và limonoid perforanon A (CA. 1996, 124 284318 m).",
        "dấu hiệu đặc trưng": "Chất perforanolion là một limonoid đã được xác minh cấu trúc (12) (CA. 1995, 122, 209740 y).",
        "triết xuất": "Một tetranortriterpenoid perforatin cũng được phân lập từ đa đa (CA.1991,114,182069 y).",
        "gồm chất": "Ngoài ra, còn có acid gallic, sitosterol và 3-O-β-D-glucopyranosyl sitosterol."
      },
      "tasteProperty": {
        "@type": "Tính vị",
        "Có vị": "Đa đa có vị đắng, tính mát, có tác dụng thanh nhiệt, giải độc."
      },
      "medicalUseList": {
        "@type": "Công dụng",
        "cong_dung_y_hoc": "Theo kinh nghiệm của nhân dân Việt Nam và một số nước Đông Nam Á (Campuchia, Philippin), cây đa đa được dùng làm thuốc chữa đau bụng, tiêu chảy, kiết lỵ, kinh nguyệt không đều, sốt rét.",
        "được_dùng": "Trong kháng chiến chống Mỹ, vỏ thân cây đa đa được các bộ đội ở Khu 5 dùng để chữa lỵ và làm thuốc điều kinh. Xí nghiệp liên hiệp Dược tỉnh Đắk Lắk đã sản xuất viên H1, gồm vỏ cây đa đa và mễ hoa trắng.",
        "làm thuốc": "Cách làm cụ thể như sau:  Vỏ thân cây đa đa phơi khô 1000g, lấy 500g thái nhỏ, nấu với 2 lít nước, lọc, rồi cô lại thành 0,5 lít cao lỏng. Lấy nốt 500g dược liệu còn lại tán nhỏ, rây bột mịn. Về phần mễ hoa trắng đã phơi khô 1000g, cũng chế biến như trên. Trộn cao lỏng và bột của hai dược liệu tiêu chuẩn, rồi thêm bột nếp sao cho được một khối lượng đủ để sản xuất 5000 viên. Liều dùng mỗi ngày từ 8–10 viên.",
        "chữa": "Ngoài ra, người ta còn lấy vỏ thân cây đa đa phối hợp với gỗ cây bách bệnh và vỏ thân cây dền phơi khô, thái nhỏ, sắc với 400ml nước còn 100ml, uống trong ngày, chữa sốt rét.",
        "sắc uống": "Ở Trung Quốc, người ta dùng rễ cây đa đa pha chế thành dạng siro cũng dùng chữa sốt rét. Ở Thái Lan, y tế đã dùng cây đa đa làm thuốc hạ sốt.  (Nguồn: Cây thuốc và động vật làm thuốc ở Việt Nam)"
      },
